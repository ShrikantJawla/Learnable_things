					<hr>
					<div class="content-wrapper">
						<div class="container-fluid">
							<div class="row">
								<div class="col-12">
									<div class="card">

										<div class="card-body">
											<div class="text-center m-5 p-5">
												<div class="col-12" style="text-align: center;">
													<form class="" id="demo-upload">
														<div class="choose-file" style="display: inline-block;">
															<input type="file" id="fileUpload" />
														</div>

														<div class="upload btn" style="display: inline-block;">
															<input type="button" id="upload" value="Upload" />
														</div>
														<br>
														<p id="errorShow"></p>
														<span style="font-size: 20px;">isForUpdate : <label class="switch">
															<input id="isForUpdate" type="checkbox">
															<span class="slider round"></span>
														  </label></span>
													</form>
												</div>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
