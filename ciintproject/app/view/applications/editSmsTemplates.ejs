<!DOCTYPE html>
<html lang="en">

<head>
	<%- include('../common/commonHeader'); %>
	<link rel="stylesheet" href="/./app/utils/assets/css/editTemplate.css">
</head>

<body>

	<div class="app align-content-stretch d-flex flex-wrap">
		<%- include('../common/commonSidebar' , {sidebarData :
            sidebarDataByServer}); %>
			<div class="app-container">
				<div class="loader-show" id="loader">
					<div class="sbl-circ-path loader-content"></div>
				</div>

				<div class="app-content">
					<h1 class="application-title">Edit / Add Message Templates</h1>
					<hr>
                    <div class="content-wrapper">
                        <div class="container">
                            <div class="row">
                                <div class="col-4" style="text-align: center;">
                                   
                                </div>
                            </div>
                        </div>
                    </div>
					<div class="content-wrapper">
						<div class="container-xl">
							<div class="table-responsive">
								<div class="table-wrapper">
									<div class="table-title">
										<div class="row">
											<div class="col-sm-6">
												<form class="" id="demo-upload">
													<div>
														<select required onchange = "FILEUPLOADDATA.selectBank(this)" class="form-select" id="card-issuers-here"
															aria-label="Default select example">
															<option value="" disabled selected>Select Bank</option>
			
														</select>
														<br>
														<br>
														<p id="errorShow"></p>
													</div>
												</form>
											</div>
											<div class="col-sm-6">
												<a href="#addEmployeeModal" class="btn btn-success rounded" data-toggle="modal"><i class="material-icons">&#xE147;</i> <span>Add Template</span></a>
											</div>
										</div>
									</div>
									<table class="table table-striped table-hover" id="template-table">
										<thead>
											<tr class="text-black">
												<th>Bank</th>
												<th>Status</th>
												<th>Flow Id</th>
												<th>Template Id</th>
												<th>Template</th>
												<th>Edit/Delete</th>
											</tr>
										</thead>
										<tbody>

										</tbody>
									</table>
								</div>
							</div>        
						</div>
						<!-- Edit Modal HTML -->
						<div id="addEmployeeModal" class="modal fade">
							<div class="modal-dialog">
								<div class="modal-content">
									<form id="add-sms-template">
										<div class="modal-header">						
											<h4 class="modal-title">Add Template</h4>
											<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
										</div>
										<div class="modal-body">					
											<div class="form-group">
												<label>Bank</label>
												<select name="bank_name" id="issuer_id_new" class="form-control" required>
													<option disabled selected value="">Select Bank</option>
													<option value="1">Axis Bank</option>
													<option value="2">Bank of Baroda</option>
													<option value="4">IDFC Bank</option>
													<option value="6">ICICI Bank</option>
													<option value="7">Au Bank</option>
													<option value="11">Yes Bank</option>
												</select>
											</div>
											<div class="form-group">
												<label>Status</label>
												<input id="sms_status_new" type="text" class="form-control" required>
											</div>
											<div class="form-group">
												<label>Flow Id</label>
												<input id="flow_id_new" type="text" class="form-control" required>
											</div>
											<div class="form-group">
												<label>Template Id</label>
												<input id="template_id_new" type="text" class="form-control" required>
											</div>
											<div class="form-group">
												<label>Template</label>
												<input id="template_new" type="text" class="form-control" required>
											</div>				
										</div>
										<div class="modal-footer">
											<input type="button" class="btn btn-default" data-dismiss="modal" value="Cancel">
											<input type="submit" class="btn btn-success" value="Add">
										</div>
									</form>
								</div>
							</div>
						</div>
						<!-- Edit Modal HTML -->
						<div id="editTemplateModal" class="modal fade">
							<div class="modal-dialog">
								<div class="modal-content">
									<form id="template-edit-form">
										<div class="modal-header">						
											<h4 class="modal-title">Edit Template</h4>
											<button type="button" class="close close-edit-modal" data-dismiss="modal" aria-hidden="true">&times;</button>
										</div>
										<div class="modal-body">
											<input hidden id="edit-id" type="text" class="form-control" required>			
											<div class="form-group">
												<label>Bank</label>
												<input disabled id="edit-bank" type="text" class="form-control" required>
											</div>
											<div class="form-group">
												<label>Status</label>
												<input id="edit-status" type="text" class="form-control" required>
											</div>
											<div class="form-group">
												<label>Flow Id</label>
												<input id="edit-flow-id" type="text" class="form-control" required>
											</div>
											<div class="form-group">
												<label>Template Id</label>
												<input id="edit-template-id" type="text" class="form-control" required>
											</div>
											<div class="form-group">
												<label>Template</label>
												<input id="edit-template" type="text" class="form-control" required>
											</div>				
										</div>
										<div class="modal-footer">
											<input type="button" class="btn btn-default close-edit-modal" data-dismiss="modal" value="Cancel">
											<input type="submit" id="save-edit-template" class="btn btn-success" disabled value="Save">
										</div>
									</form>
								</div>
							</div>
						</div>
						<!-- Delete Modal HTML -->
						<div id="deleteTemplateModal" class="modal fade">
							<div class="modal-dialog">
								<div class="modal-content">
									<form>
										<div class="modal-header">						
											<h4 class="modal-title">Delete Employee</h4>
											<button type="button" class="close close-delete-modal" data-dismiss="modal" aria-hidden="true">&times;</button>
										</div>
										<div class="modal-body">					
											<p>Are you sure you want to delete this Template?</p>
										</div>
										<div class="modal-footer">
											<input type="button" class="btn btn-default close-delete-modal" data-dismiss="modal" value="Cancel">
											<input type="submit" id="delete-template-btn" class="btn btn-danger" value="Delete">
										</div>
									</form>
								</div>
							</div>
						</div>
					</div>
				</div>


				<!-- Javascripts -->
				<script src="/./app/utils/assets/plugins/jquery/jquery-3.5.1.min.js"></script>
				<script src="/./app/utils/assets/plugins/bootstrap/js/popper.min.js"></script>
				<script src="/./app/utils/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
				<script src="/./app/utils/assets/plugins/perfectscroll/perfect-scrollbar.min.js"></script>
				<script src="/./app/utils/assets/plugins/pace/pace.min.js"></script>
				<script src="/./app/utils/assets/plugins/highlight/highlight.pack.js"></script>
				<script src="/./app/utils/assets/plugins/dropzone/min/dropzone.min.js"></script>
				<script src="/./app/utils/assets/js/main.min.js"></script>
				<script src="/./app/utils/assets/js/custom.js"></script>
				<script src="/./app/utils/assets/js/custom/matchPhoneNumberAndUpload.js"></script>
				<script src="/./app/utils/assets/js/custom/editTemplate.js"></script>
				

</body>

</html>
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css">
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/xlsx/0.13.5/xlsx.full.min.js"></script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/xlsx/0.13.5/jszip.js"></script>
<script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js"></script>
