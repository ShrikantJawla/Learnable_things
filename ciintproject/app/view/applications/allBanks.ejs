<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../common/commonHeader'); %>
        <!-- <link href="/./app/utils/assets/plugins/flatpickr/flatpickr.min.css" rel="stylesheet" /> -->
</head>

<body>
    <div class="app align-content-stretch d-flex flex-wrap">
        <%- include('../common/commonSidebar' , {sidebarData :
            sidebarDataByServer}); %>
            <div class="app-container">
                <div class="loader-show" id="loader1">
                    <div class="sbl-circ-path loader-content"></div>
                </div>
                <div class="d-flex flex-column ">
                    <div class="app-content">
                        <h1 class="application-title pb-1">
                            All Banks
                        </h1>
                    </div>
                    <hr>
                    <div class="card ci-scrollX p-4">
                        <header class=" card-header d-flex justify-content-between">
                            <div class="d-flex align-items-center" style="margin-bottom: 20px; gap: 12px">
                                <select name="entriesPerPage" id="entriesPerPage">
                                    <option value="5">5</option>
                                    <option value="10">10</option>
                                    <option value="25">25</option>
                                    <option value="50">50</option>
                                    <option value="100">100</option>
                                    <option value="200">200</option>
                                </select>
                                <label for="entriesPerPage" style="color: rgb(128, 128, 128)">Entries/Page</label>
                            </div>
                            <icon class="material-icons material-symbols-outlined" style="cursor: pointer;"
                                data-bs-toggle="collapse" href="#collapseExample" role="button">
                                filter_list
                            </icon>
                        </header>
                        <div class="collapse" id="collapseExample">
                            <div class="card card-body ">
                                <div class="filterBox" id="table-filter-row">
                                </div>
                            </div>
                        </div>
                        <table class="table">
                            <thead style="color: rgb( 128, 128, 128) !important;">

                                <tr style="text-align: center" id="table-header" data-filter-value="0"
                                    data-filter="none">
                                    <th data-filter-value="0" data-filter="id"
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="id">ID</span>
                                            <icon data-filter="id"
                                                class="material-icons material-symbols-outlined invisible"
                                                style="cursor: pointer;">
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    <th data-filter-value="0" data-filter="bank_name"
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="bank_name">Bank Name</span>
                                            <icon data-filter="bank_name"
                                                class="material-icons material-symbols-outlined invisible"
                                                style="cursor: pointer;">
                                                straight
                                            </icon>
                                        </div>
                                    </th>
									<th data-filter-value="0" data-filter="is_apply_active"
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="is_apply_active">Apply Active</span>
                                            <icon data-filter="is_apply_active"
                                                class="material-icons material-symbols-outlined invisible"
                                                style="cursor: pointer;">
                                                straight
                                            </icon>
                                        </div>
                                    </th>
									<th data-filter-value="0" data-filter="is_sms_active"
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="is_sms_active">SMS Active</span>
                                            <icon data-filter="is_sms_active"
                                                class="material-icons material-symbols-outlined invisible"
                                                style="cursor: pointer;">
                                                straight
                                            </icon>
                                        </div>
                                    </th>
									<th data-filter-value="0" data-filter="created_at"
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="created_at">Created at</span>
                                            <icon data-filter="created_at"
                                                class="material-icons material-symbols-outlined invisible"
                                                style="cursor: pointer;">
                                                straight
                                            </icon>
                                        </div>
                                    </th>
									<th data-filter-value="0" data-filter="updated_at"
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="updated_at">Updated at</span>
                                            <icon data-filter="updated_at"
                                                class="material-icons material-symbols-outlined invisible"
                                                style="cursor: pointer;">
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    
                                </tr>
                            </thead>
                            <tbody id="data-to-show">
                                <tr>
                                    <td colspan="12">
                                        <div class="d-flex justify-content-center align-items-center">
                                            <div class="sbl-circ-path"></div>
                                        </div>
                                    </td>
                                </tr>
                            </tbody>
                            <tfoot>
                                <tr>
                                    <td colspan="12" style="text-align: center;  color: black !important;">
                                        <div
                                            class="display: flex; align-items: center; justify-content: center; flex-direction: column;">

                                            <span class="material-icons material-symbols-outlined"
                                                style=" position: relative; top: 4px;right: 8px; font-size: 18px; color: white; background: rgb( 128, 128,128) !important; padding: 2px;border-radius: 50%; cursor: pointer;"
                                                id="prevPage">
                                                west
                                            </span>
                                            <%# total pages label %>
                                                <label for="pageNo" style="
                                                margin-right: 10px;
                                                color: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                            ">Page No:</label>
                                                <%# input %>
                                                    <input type="number" style="
                                                width: 50px;
                                                display: inline;
                                                height: 20px;
                                                border-radius: 0 !important;
                                                padding-right: 0 !important;
                                                padding-left: 2px !important;
                                            " id="page-no" name="pageNo" class="form-control" max="50" min="1"
                                                        value="1" />
                                                    <%# /50 %>

                                                        <span style="
                                                margin-left: 10px;
                                                color: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                            " id="pages">/ 50</span>
                                                        <%# no of entries %>

                                                            <span style="
                                                margin-left: 10px;
                                                color: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                            " id="entries">(2000 entries)</span>

                                                            <span class="material-icons material-symbols-outlined"
                                                                style="
                                                position: relative;
                                                top: 4px;
                                                left: 8px;
                                                font-size: 18px;
                                                color: white;
                                                background: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                                padding: 2px;
                                                border-radius: 50%;
                                                cursor: pointer;
                                            " id="nextPage">
                                                                east
                                                            </span>
                                        </div>
                                    </td>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
                <!-- Javascripts -->
                <script src="/./app/utils/assets/plugins/jquery/jquery-3.5.1.min.js"></script>
                <script src="/./app/utils/assets/plugins/bootstrap/js/popper.min.js"></script>
                <script src="/./app/utils/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
                <script src="/./app/utils/assets/plugins/perfectscroll/perfect-scrollbar.min.js"></script>
                <script src="/./app/utils/assets/plugins/pace/pace.min.js"></script>
                <script src="/./app/utils/assets/plugins/highlight/highlight.pack.js"></script>
                <script src="/./app/utils/assets/plugins/datatables/datatables.min.js"></script>
                <script src="/./app/utils/assets/js/main.min.js"></script>
                <script src="/./app/utils/assets/js/custom.js"></script>
                <script src="/./app/utils/assets/js/pages/datatables.js"></script>
                <script src="/./app/utils/assets/plugins/flatpickr/flatpickr.js"></script>
                <script src="/./app/utils/assets/js/custom/common.js"></script>
                <script src="/./app/utils/assets/js/custom/allbanks.js"></script>
</body>

</html>