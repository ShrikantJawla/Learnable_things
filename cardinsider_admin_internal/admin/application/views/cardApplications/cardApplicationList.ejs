<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('../common/commonHeader'); %>
        <link
            rel="stylesheet"
            href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css"
            integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g=="
            crossorigin="anonymous"
            referrerpolicy="no-referrer"
        />
        <link
            href="./admin/assets/plugins/select2/css/select2.min.css"
            rel="stylesheet"
        />
        <link
            href="./admin/assets/plugins/flatpickr/flatpickr.min.css"
            rel="stylesheet"
        />
    </head>

    <body>
        <div class="app align-content-stretch d-flex flex-wrap">
            <%- include('../common/commonSidebar' , {sidebarData :
            sidebarDataByServer}); %>

            <div class="app-container ci-table-padding">
                <div class="d-flex flex-column">
                    <div class="app-content">
                        <h1 class="application-title pb-1">Card Application</h1>
                    </div>
                    <div
                        style="
                            display: flex;
                            margin-bottom: 10px;
                            gap: 10px;
                            margin-left: auto;
                        "
                    >
                        <button class="btn btn-success" id="filterReset">
                            Reset Filters
                        </button>
                        <button
                            class="btn btn-primary"
                            onclick="window.location='/new-card-application'"
                        >
                            Add Item
                        </button>
                        <button class="btn btn-info">Export csv</button>
                    </div>
                </div>
                <div class="card ci-scrollX">
                    <header class="card-header d-flex justify-content-between">
                        <div
                            class="d-flex align-items-center"
                            style="margin-bottom: 20px; gap: 12px"
                        >
                            <select
                                name="entriesPerPage"
                                id="entriesPerPage"
                                style="
                                    color: rgb(128, 128, 128) !important;
                                    border-color: rgb(128, 128, 128) !important;
                                "
                            >
                                <option value="10">10</option>
                                <option value="25">25</option>
                                <option value="50">50</option>
                                <option value="100">100</option>
                                k
                                <option value="200">200</option>
                                <option value="500">500</option>
                            </select>
                            <label
                                for="entriesPerPage"
                                style="color: rgb(128, 128, 128)"
                            >
                                entries / page</label
                            >
                        </div>
                        <icon
                            class="material-icons material-symbols-outlined"
                            style="cursor: pointer"
                            data-bs-toggle="collapse"
                            href="#table-filter-row"
                            role="button"
                        >
                            filter_list
                        </icon>
                    </header>
                    <table class="table">
                        <thead style="color: rgb(128, 128, 128) !important">
                            <tr
                                style="text-align: center"
                                id="table-header"
                                data-filter-value="0"
                                data-filter="none"
                            >
                                <th
                                    data-filter-value="0"
                                    data-filter="id"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                    <div class="d-flex">
                                        <span
                                            style="cursor: pointer"
                                            data-filter="id"
                                            >ID</span
                                        >
                                        <icon
                                            data-filter="id"
                                            class="material-icons material-symbols-outlined invisible"
                                            style="cursor: pointer"
                                        >
                                            straight
                                        </icon>
                                    </div>
                                </th>

                                <th
                                    data-filter-value="0"
                                    data-filter="card_insider_user"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                    <div class="d-flex">
                                        <span
                                            style="cursor: pointer"
                                            data-filter="card_insider_user"
                                            >CI User</span
                                        >
                                        <icon
                                            data-filter="card_insider_user"
                                            class="material-icons material-symbols-outlined invisible"
                                            style="cursor: pointer"
                                        >
                                            straight
                                        </icon>
                                    </div>
                                </th>
                                <th
                                    data-filter-value="0"
                                    data-filter="user_name"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                    <div class="d-flex">
                                        <span
                                            style="cursor: pointer"
                                            data-filter="user_name"
                                            >UserName</span
                                        >
                                        <icon
                                            data-filter="user_name"
                                            class="material-icons material-symbols-outlined invisible"
                                            style="cursor: pointer"
                                        >
                                            straight
                                        </icon>
                                    </div>
                                </th>
                                <th
                                    data-filter-value="0"
                                    data-filter="Application_number"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                    <div class="d-flex">
                                        <span
                                            style="cursor: pointer"
                                            data-filter="Application_number"
                                            >Application Number</span
                                        >
                                        <icon
                                            data-filter="Application_number"
                                            class="material-icons material-symbols-outlined invisible"
                                            style="cursor: pointer"
                                        >
                                            straight
                                        </icon>
                                    </div>
                                </th>
                                <th
                                    data-filter-value="0"
                                    data-filter="user_number"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                    <div class="d-flex">
                                        <span
                                            style="cursor: pointer"
                                            data-filter="user_number"
                                            >Contact</span
                                        >
                                        <icon
                                            data-filter="user_number"
                                            class="material-icons material-symbols-outlined invisible"
                                            style="cursor: pointer"
                                        >
                                            straight
                                        </icon>
                                    </div>
                                </th>
                                <th
                                    data-filter-value="0"
                                    data-filter="IssuerName"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                    <div class="d-flex">
                                        <span
                                            data-filter="IssuerName"
                                            style="cursor: pointer"
                                            >CI</span
                                        >
                                        <icon
                                            data-filter="IssuerName"
                                            class="material-icons material-symbols-outlined invisible"
                                            style="cursor: pointer"
                                        >
                                            straight
                                        </icon>
                                    </div>
                                </th>
                                <th
                                    data-filter-value="0"
                                    data-filter="CreditCardName"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                    <div class="d-flex">
                                        <span
                                            style="cursor: pointer"
                                            data-filter="CreditCardName"
                                            >CreditCard</span
                                        >
                                        <icon
                                            data-filter="CreditCardName"
                                            class="material-icons material-symbols-outlined invisible"
                                            style="cursor: pointer"
                                        >
                                            straight
                                        </icon>
                                    </div>
                                </th>
                                <th
                                    data-filter-value="0"
                                    data-filter="application_through"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                    <div class="d-flex">
                                        <span
                                            style="cursor: pointer"
                                            data-filter="application_through"
                                            >Mode</span
                                        >
                                        <icon
                                            data-filter="application_through"
                                            class="material-icons material-symbols-outlined invisible"
                                            style="cursor: pointer"
                                        >
                                            straight
                                        </icon>
                                    </div>
                                </th>
                                <th
                                    data-filter-value="0"
                                    data-filter="Application_Status"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                    <div class="d-flex">
                                        <span
                                            style="cursor: pointer"
                                            data-filter="Application_Status"
                                            >Status</span
                                        >
                                        <icon
                                            data-filter="Application_Status"
                                            class="material-icons material-symbols-outlined invisible"
                                            style="cursor: pointer"
                                        >
                                            straight
                                        </icon>
                                    </div>
                                </th>
                                <th
                                    data-filter-value="0"
                                    data-filter="Application_date"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                    <div class="d-flex">
                                        <span
                                            style="cursor: pointer"
                                            data-filter="Application_date"
                                        >
                                            AppliedOn</span
                                        >
                                        <icon
                                            data-filter="Application_date"
                                            class="material-icons material-symbols-outlined invisible"
                                            style="cursor: pointer"
                                        >
                                            straight
                                        </icon>
                                    </div>
                                </th>
                                <th
                                    data-filter-value="0"
                                    data-filter="Cashback_paid"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                    <div class="d-flex">
                                        <span
                                            data-filter="Cashback_paid"
                                            style="cursor: pointer"
                                            >Cashback</span
                                        >
                                        <icon
                                            data-filter="Cashback_paid"
                                            class="material-icons material-symbols-outlined invisible"
                                            style="cursor: pointer"
                                        >
                                            straight
                                        </icon>
                                    </div>
                                </th>
                                <!-- <th
                                    data-filter-value="0"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                    <div class="d-flex">
                                        <span>Actions</span>
                                    </div>
                                </th> -->
                            </tr>
                        </thead>
                        <tr
                            id="table-filter-row"
                            style="
                                border-bottom: 1px solid rgb(199, 199, 199);
                                background: #e4ebfd;
                            "
                        >
                            <!-- Id -->
                            <th>
                                <input
                                    type="text"
                                    name="id"
                                    class="form-control"
                                    id="id"
                                />
                            </th>
                            <!-- card_insider_user -->
                            <th>
                                <input
                                    type="text"
                                    name="card_insider_user"
                                    class="form-control"
                                    id="card_insider_user"
                                />
                            </th>
                            <!-- user_name -->
                            <th>
                                <input
                                    type="text"
                                    name="user_name"
                                    class="form-control"
                                    id="user_name"
                                />
                            </th>
                            <!-- Application_number -->
                            <th>
                                <input
                                    type="text"
                                    name="Application_number"
                                    class="form-control"
                                    id="Application_number"
                                />
                            </th>
                            <!-- Contact -->
                            <th>
                                <input
                                    type="text"
                                    id="user_number"
                                    name="user_number"
                                    class="form-control"
                                />
                            </th>
                            <!-- card_issuer -->
                            <th>
                                <select
                                    id="card-issuer-options"
                                    name="card_issuer"
                                    class="form-control"
                                    style="min-width: 70px; padding: 10px 5px"
                                ></select>
                            </th>
                            <th>
                                <select
                                    id="credit-card-options"
                                    name="credit_card"
                                    class="form-control"
                                    style="min-width: 70px; padding: 10px 5px"
                                ></select>
                            </th>
                            <th>
                                <select
                                    id="application_through-options"
                                    name="application_through"
                                    class="form-control"
                                    style="min-width: 70px; padding: 10px 5px"
                                >
                                    <option selected value="any">Select</option>
                                    <option value="form">Form</option>
                                    <option value="ciapp">App</option>
                                    <option value="web">Web</option>
                                </select>
                            </th>
                            <th>
                                <select
                                    id="application_status-options"
                                    name="Application_Status"
                                    class="form-control"
                                    style="min-width: 70px; padding: 10px 5px"
                                >
                                    <option selected value="any">Select</option>
                                    <option value="Approved">Approved</option>
                                    <option value="Rejected">Rejected</option>
                                    <option value="Processing">
                                        Processing
                                    </option>
                                </select>
                            </th>
                            <th>
                                <div class="d-flex" style="margin-bottom: 2px">
                                    <span
                                        style="
                                            font-size: 12px;
                                            width: 40px;
                                            color: rgb(
                                                128,
                                                128,
                                                128
                                            ) !important;
                                        "
                                        >From</span
                                    >
                                    <input
                                        type="date"
                                        id="from_application_date"
                                        name="from_application_date"
                                        class="form-control"
                                        placeholder="yyyy-mm-dd"
                                        style="
                                            width: 90px;
                                            padding: 0;
                                            height: 20px;
                                            border-radius: 3px;
                                            font-size: 11px;
                                        "
                                    />
                                </div>

                                <div class="d-flex">
                                    <span
                                        style="
                                            font-size: 12px;
                                            width: 40px;
                                            color: rgb(
                                                128,
                                                128,
                                                128
                                            ) !important;
                                        "
                                        >To
                                    </span>
                                    <input
                                        type="date"
                                        id="to_application_date"
                                        name="to_application_date"
                                        class="form-control"
                                        placeholder="yyyy-mm-dd"
                                        style="
                                            width: 90px;
                                            padding: 0;
                                            height: 20px;
                                            border-radius: 3px;
                                            font-size: 11px;
                                        "
                                    />
                                </div>
                            </th>
                            <th>
                                <select
                                    id="Cashback_paid-options"
                                    name="Cashback_paid"
                                    class="form-control"
                                    style="min-width: 70px; padding: 10px 5px"
                                >
                                    <option selected value="any">Select</option>
                                    <option value="true">Yes</option>
                                    <option value="false">No</option>
                                </select>
                            </th>
                        </tr>
                        <tbody id="data-to-show">
                            <tr>
                                <td colspan="10">
                                    <div
                                        class="d-flex justify-content-center align-items-center"
                                    >
                                        <div class="sbl-circ-path"></div>
                                    </div>
                                </td>
                            </tr>
                        </tbody>
                        <tfoot>
                            <tr>
                                <td
                                    colspan="10"
                                    style="
                                        text-align: center;
                                        color: black !important;
                                    "
                                >
                                    <div
                                        class="display: flex; align-items: center; justify-content: center; flex-direction: column;"
                                    >
                                        <span
                                            class="material-icons material-symbols-outlined"
                                            style="
                                                position: relative;
                                                top: 4px;
                                                right: 8px;
                                                font-size: 18px;
                                                color: white;
                                                background: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                                padding: 2px;
                                                border-radius: 50%;
                                                cursor: pointer;
                                            "
                                            id="prevPage"
                                        >
                                            west
                                        </span>
                                        <label
                                            for="pageNo"
                                            style="
                                                margin-right: 10px;
                                                color: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                            "
                                            >Page No:</label
                                        >
                                        <input
                                            type="number"
                                            style="
                                                width: 50px;
                                                display: inline;
                                                height: 20px;
                                                border-radius: 0 !important;
                                                padding-right: 0 !important;
                                                padding-left: 2px !important;
                                            "
                                            id="page-no"
                                            name="pageNo"
                                            class="form-control"
                                            max="50"
                                            min="1"
                                            value="1"
                                        />
                                        <span
                                            style="
                                                margin-left: 10px;
                                                color: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                            "
                                            id="pages"
                                            >/ 50</span
                                        >
                                        <span
                                            style="
                                                margin-left: 10px;
                                                color: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                            "
                                            id="entries"
                                            >(2000 entries)</span
                                        >
                                        <span
                                            class="material-icons material-symbols-outlined"
                                            style="
                                                position: relative;
                                                top: 4px;
                                                left: 8px;
                                                font-size: 18px;
                                                color: white;
                                                background: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                                padding: 2px;
                                                border-radius: 50%;
                                                cursor: pointer;
                                            "
                                            id="nextPage"
                                        >
                                            east
                                        </span>
                                    </div>
                                </td>
                            </tr>
                        </tfoot>
                    </table>
                </div>
            </div>
        </div>

        <!-- Javascripts -->
        <script src="./admin/assets/plugins/jquery/jquery-3.5.1.min.js"></script>
        <script src="./admin/assets/plugins/bootstrap/js/popper.min.js"></script>
        <script src="./admin/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
        <script src="./admin/assets/plugins/perfectscroll/perfect-scrollbar.min.js"></script>
        <script src="./admin/assets/plugins/pace/pace.min.js"></script>
        <script src="./admin/assets/plugins/highlight/highlight.pack.js"></script>
        <script src="./admin/assets/plugins/datatables/datatables.min.js"></script>
        <script src="./admin/assets/js/main.min.js"></script>
        <script src="./admin/assets/js/custom.js"></script>
        <script src="./admin/assets/js/applications.js"></script>
        <script src="./admin/assets/plugins/select2/js/select2.full.min.js"></script>
        <script src="./admin/assets/js/pages/datatables.js"></script>
        <script src="./admin/assets/plugins/flatpickr/flatpickr.js"></script>
    </body>
</html>
