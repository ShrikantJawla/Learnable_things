<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../common/commonHeader'); %>
        <link href="./admin/assets/plugins/flatpickr/flatpickr.min.css" rel="stylesheet">
</head>

<body>
<!--  -->
    <div class="app align-content-stretch d-flex flex-wrap">
      <%- include('../common/commonSidebar' , {sidebarData : sidebarDataByServer}); %>
            <%- include('../common/commonModals'); %>
                <div class="app-container">
                    <div class="app-content">
                        <div class="content-wrapper">
                            <div class="container-fluid">
                                <div class="row">
                                    <div class="col">
                                        <div class="app-content">
                                            <h1 class="application-title  bold">
                                                New Application
                                            </h1>
                                            <div class="top-btns">
                                                <button class="ci-btn ci-btn-green" id="save-application" disabled  data-bs-toggle="modal" data-bs-target="#saveModal">Save</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="card bg-card">
                                        <form >
                                            <div class="ci-card-body newoffer card-body">
                                                <div class="col-8 shadowBox p-4" id="left-form" style="min-height:300px;">
                                                    <div class="row">
                                                        <div class="col-6">
                                                            <label for="" class="form-label ">Application Number</label>
                                                            <input type="text" class="form-control" id="Application_number" name="Application_number" required>
                                                        </div>
                                                        <div class="col-6">
                                                            <label for="Phone_Number" class="form-label ">Phone Number</label>
                                                            <input type="text" class="form-control" id="Phone_Number" name="Phone_Number" required>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-3">
                                                        <div class="col-6">
                                                            <label for="Application_Status" class="form-label ">Application Status</label>
                                                            <select name="Application_Status" id="Application_Status" class="form-control">
                                                                <option value="">Select</option>
                                                                <option value="Approved">Approved</option>
                                                                <option value="Processing">Processing</option>
                                                                <option value="Rejected">Rejected</option>
                                                            </select>
                                                        </div>
                                                        <div class="col-6">
                                                            <label for="Application_date" class="form-label ">Application Date</label>
                                                            <input type="date" class="form-control" id="Application_date" 
                                                            placeholder="yyyy-mm-dd"
                                                            name="Application_date" required>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-3">
                                                        <div class="col-6">
                                                            <label for="Cashback_to_be_paid" class="form-label">Cashback To Be Paid</label>
                                                            <input type="text" class="form-control" id="Cashback_to_be_paid" name="Cashback_to_be_paid" required>
                                                        </div>
                                                        
                                                        <div class="col-6">
                                                            <label for="Referral_commission_to_be_paid" class="form-label">Referral Commission To Be Paid</label>
                                                            <input type="text" class="form-control" id="Referral_commission_to_be_paid" name="Referral_commission_to_be_paid" required>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-3">
                                                        <div class="col-6">
                                                            <label for="tracking_id" class="form-label">Tracking Id</label>
                                                            <input type="text" class="form-control" id="tracking_id" name="tracking_id" required>
                                                        </div>
                                                        <div class="col-6">
                                                            <label for="ipa_status" class="form-label">IPA Status</label>
                                                            <input type="text" class="form-control" id="ipa_status" name="ipa_status" required>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-3">
                                                        <div class="col-6">
                                                            <label for="application_through" class="form-label ">Application Through</label>
                                                            <select name="application_through" id="application_through" class="form-control">
                                                                <option value="">Select</option>
                                                                <option value="ciapp">ciapp</option>
                                                                <option value="form">form</option>
                                                                <option value="web">web</option>
                                                            </select>
                                                        </div>
                                                        <div class="col-6">
                                                            <label for="application_reason" class="form-label ">Application Reason</label>
                                                            <input class="form-control" id="application_reason" name="application_reason" required>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-3">
                                                         <div class="col-6">
                                                            <label for="all_unique_number" class="form-label ">Application Unique Number</label>
                                                            <input class="form-control" id="all_unique_number" name="all_unique_number" required>
                                                        </div>
                                                         
                                                    </div>
                                                    <div class="row mt-3">
                                                        <div class="col-12" >
                                                            <label for="application_notes" class="form-label ">Application Notes</label>
                                                            <textarea class="form-control" id="application_notes" 
                                                            style="min-height:44px ;"name="application_notes"></textarea>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-4">
                                                        <div class="col-3">
                                                            <label for="Cashback_applicable" class="form-label ">Cashback Applicable</label>
                                                            <label class="switch">
                                                                <input type="checkbox" id="Cashback_applicable">
                                                                <span class="slider round"></span>
                                                            </label>
                                                        </div>
                                                        <div class="col-3" 
                                                        style="
                                                            display: flex;
                                                            flex-direction: column;
                                                            justify-content: center;
                                                        ">
                                                            <label for="Cashback_paid" class="form-label ">Cashback Paid</label>
                                                            <label class="switch">
                                                                <input type="checkbox" id="Cashback_paid">
                                                                <span class="slider round"></span>
                                                            </label>
                                                        </div>
                                                        <div class="col-3">
                                                            <label for="Ref_commission_paid" class="form-label ">Reference Commission</label>
                                                            <label class="switch">
                                                                <input type="checkbox" id="Ref_commission_paid">
                                                                <span class="slider round"></span>
                                                            </label>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-4 " style="padding-left: 28px!important;">
                                                    <div class="requiredWrapper ">
                                                            <div class=""> <span class="required">*</span><span>Required</span></div>
                                                            <!-- <div class=""><span class="oneofrequired">**</span><span>One of the marked is required</span></div> -->
                                                    </div>
                                                    <div class=" form-group d-flex flex-column gap-3 mt-4 " id="multiSelectWrapper">
                                                            <div class="selectBox " id="cardIssuersSelectBox">
                                                                <label class="multiInputLabel" id="cardIssuersLabel">
                                                                    Card Issuer
                                                                </label>
                                                                <input type="text" name="cardIssuersInput" id="cardIssuersInput" class="multiSelectInput form-control" placeholder="Select a Card Issuer" autocomplete="off" />
                                                                <button class="material-icons material-symbols-outlined input-expand-icon" id="cardIssuersDropdownIcon">
                                                                    expand_more
                                                                </button>
                                                                <div id="cardIssuersDropdown" class="selectBoxDropdown dropdownHidden "></div>
                                                                <div class="selectBoxSelected" id="cardIssuersSelectBoxSelected"></div>
                                                            </div>
                                                            <div class="selectBox" id="creditCardsSelectBox" >
                                                                <label 
                                                                    class="multiInputLabel"
                                                                    id="creditCardsLabel"
                                                                >
                                                                    Credit Card (0)
                                                                    
                                                                </label>
                                                                <input
                                                                    type="text"
                                                                    name="creditCardsInput"
                                                                    id="creditCardsInput"
                                                                    class="multiSelectInput  form-control"
                                                                    placeholder="Select a Credit Card"
                                                                    autocomplete="off"
                                                                />
                                                                <button 
                                                                    class="material-icons material-symbols-outlined input-expand-icon"
                                                                    id="creditCardsDropdownIcon"
                                                                >
                                                                    expand_more
                                                                </button>
                                                                <div
                                                                    id="creditCardsDropdown"
                                                                    class="selectBoxDropdown dropdownHidden "
                                                                ></div>
                                                                <div class="selectBoxSelected" id="creditCardsSelectBoxSelected"></div>
                                                            </div>
                                                            <div class="selectBox" id="ciUsersSelectBox" >
                                                                <label 
                                                                    class="multiInputLabel"
                                                                    id="ciUsersLabel"
                                                                >
                                                                    Card Insider Users
                                                                </label>
                                                                <input
                                                                    type="text"
                                                                    name="ciUsersInput"
                                                                    id="ciUsersInput"
                                                                    class="multiSelectInput  form-control"
                                                                    placeholder="Select an item"
                                                                    autocomplete="off"
                                                                />
                                                                <button 
                                                                    class="material-icons material-symbols-outlined input-expand-icon"
                                                                    id="ciUsersDropdownIcon"
                                                                >
                                                                    expand_more
                                                                </button>
                                                                <div
                                                                    id="ciUsersDropdown"
                                                                    class="selectBoxDropdown dropdownHidden "
                                                                ></div>
                                                                <div class="selectBoxSelected" id="ciUsersSelectBoxSelected"></div>
                                                            </div>
                                                            <div class="selectBox" id="refferedBySelectBox" >
                                                                <label 
                                                                    class="multiInputLabel"
                                                                    id="refferedByLabel"
                                                                >
                                                                    Reffered By
                                                                </label>
                                                                <input
                                                                    type="text"
                                                                    name="refferedByInput"
                                                                    id="refferedByInput"
                                                                    class="multiSelectInput  form-control"
                                                                    placeholder="Select an item"
                                                                    autocomplete="off"
                                                                />
                                                                <button 
                                                                    class="material-icons material-symbols-outlined input-expand-icon"
                                                                    id="refferedByDropdownIcon"
                                                                >
                                                                    expand_more
                                                                </button>
                                                                <div
                                                                    id="refferedByDropdown"
                                                                    class="selectBoxDropdown dropdownHidden "
                                                                ></div>
                                                                <div class="selectBoxSelected" id="refferedBySelectBoxSelected"></div>
                                                            </div>
                                                            
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Javascripts -->
                <script src="./admin/assets/plugins/jquery/jquery-3.5.1.min.js"></script>
                <script src="./admin/assets/plugins/bootstrap/js/popper.min.js"></script>
                <script src="./admin/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
                <script src="./admin/assets/plugins/perfectscroll/perfect-scrollbar.min.js"></script>
                <script src="./admin/assets/plugins/flatpickr/flatpickr.js"></script>
                <script src="./admin/assets/plugins/pace/pace.min.js"></script>
                <script src="./admin/assets/plugins/highlight/highlight.pack.js"></script>
                <script src="./admin/assets/js/main.min.js"></script>
                <script src="./admin/assets/js/custom.js"></script>
                <script src="./admin/assets/js/utils/blockModalsEsacping.js"></script>
                <script src="./admin/assets/js/utils/multiUniSelectObject.js"></script>
                <script src="./admin/assets/js/newApplication.js"></script>

</body>

</html>