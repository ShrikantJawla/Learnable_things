<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('../common/commonHeader'); %>
        <link
            rel="stylesheet"
            href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css"
            integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g=="
            crossorigin="anonymous"
            referrerpolicy="no-referrer"
        />
    </head>

    <body>
        <div class="app align-content-stretch d-flex flex-wrap">
            <%- include('../common/commonSidebar' , {sidebarData :
            sidebarDataByServer}); %> <%- include('../common/commonModals'); %>
            <%- include('../common/commonLoader'); %>
            <div class="app-container">
                <div class="app-content">
                    <div class="content-wrapper">
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col">
                                    <div class="app-content">
                                        <h1 class="application-title bold">
                                            Credit Card Sequences
                                        </h1>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-6">
                                    <div class="top-btns">
                                        <button
                                            class="ci-btn ci-btn-new ci-btn-blue"
                                            id="save-apply-sequence"
                                        >
                                            Save
                                        </button>
                                        <button
                                            class="ci-btn ci-btn-new ci-btn-green"
                                            id="apply-sequence-reset"
                                        >
                                            Reset
                                        </button>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <div class="top-btns">
                                        <button
                                            class="ci-btn ci-btn-new ci-btn-blue"
                                            id="save-card-sequence"
                                        >
                                            Save
                                        </button>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="card bg-card">
                                    <form>
                                        <div
                                            class="ci-card-body newoffer card-body"
                                        >
                                            <div
                                                class="col shadowBox py-4"
                                                id="left-form"
                                            >
                                                <div class="row">
                                                    <div
                                                        class="col-6"
                                                        style="
                                                            text-align: center;
                                                            border-right: 1px
                                                                solid
                                                                rgb(
                                                                    219,
                                                                    219,
                                                                    219
                                                                );
                                                            min-height: 60vh;
                                                        "
                                                    >
                                                        <h6>Apply Sequence</h6>
                                                        <div
                                                            class="apply-sequence-container"
                                                        >
                                                            <div
                                                                class="draggable-field-container-apply-sequence"
                                                            ></div>
                                                        </div>
                                                    </div>
                                                    <div
                                                        class="col-6"
                                                        style="
                                                            text-align: center;
                                                            display: flex;
                                                            flex-direction: column;
                                                            align-items: center;
                                                        "
                                                    >
                                                        <h6>
                                                            Card Sequence In All
                                                            Cards
                                                        </h6>
                                                        <select
                                                            name="ciSelect"
                                                            id="ciSelect"
                                                            class="form-select mt-2"
                                                        ></select>
                                                        <div
                                                            class="sequence-container hidden"
                                                        >
                                                            <div
                                                                class="draggable-field-container-sequence"
                                                            ></div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <script src="./admin/assets/plugins/jquery/jquery-3.5.1.min.js"></script>
        <script src="./admin/assets/plugins/bootstrap/js/popper.min.js"></script>
        <script src="./admin/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
        <script src="./admin/assets/plugins/perfectscroll/perfect-scrollbar.min.js"></script>
        <script src="./admin/assets/plugins/pace/pace.min.js"></script>
        <script src="./admin/assets/plugins/highlight/highlight.pack.js"></script>
        <script src="./admin/assets/js/main.min.js"></script>
        <script src="./admin/assets/js/custom.js"></script>
        <script src="./admin/assets/plugins/flatpickr/flatpickr.js"></script>
        <script src="./admin/assets/js/utils/blockModalsEsacping.js"></script>
        <script src="./admin/assets/js/pages/creditCardSequences.js"></script>
    </body>
</html>
