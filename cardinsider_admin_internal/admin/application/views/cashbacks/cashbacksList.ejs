<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../common/commonHeader'); %>
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css"
            integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g=="
            crossorigin="anonymous" referrerpolicy="no-referrer" />
        <link href="./admin/assets/plugins/flatpickr/flatpickr.min.css" rel="stylesheet" />
</head>

<body>
    <div class="app align-content-stretch d-flex flex-wrap">
        <%- include('../common/commonSidebar' , {sidebarData : sidebarDataByServer}); %>
            <%- include('../common/commonModals'); %>
                <div class="app-container ci-table-padding">
                    <div class="d-flex flex-column">
                        <div class="app-content">
                            <h1 class="application-title pb-1">
                                Cashback/Rewards
                            </h1>
                        </div>
                        <div style="
                            display: flex;
                            margin-bottom: 10px;
                            gap: 10px;
                            margin-left: auto;
                        ">
                            <button class="btn btn-success" id="filterReset">
                                Reset Filters
                            </button>
                            <button class="btn btn-info">Export csv</button>
                        </div>
                    </div>
                    <div class="card ci-scrollX">
                        <header class="card-header d-flex justify-content-between">
                            <div class="d-flex align-items-center" style="margin-bottom: 20px; gap: 12px">
                                <select name="entriesPerPage" id="entriesPerPage">
                                    <option value="5">5</option>
                                    <option value="10">10</option>
                                    <option value="25">25</option>
                                    <option value="50">50</option>
                                    <!-- <option value="100">100</option> -->
                                    <!-- <option value="200">200</option> -->
                                </select>
                                <label for="entriesPerPage" style="color: rgb(128, 128, 128)">
                                    entries / page</label>
                            </div>
                            <icon class="material-icons material-symbols-outlined" style="cursor: pointer;"
                                data-bs-toggle="collapse" href="#table-filter-row" role="button">
                                filter_list
                            </icon>

                        </header>
                        <!-- <div class="collapse" id="collapseExample">
                            <div class="card card-body ">
                                <div class="filterBox">
                                    <div class="container" >
                                        
                                    </div>
                                   

                                </div>
                            </div>
                    </div> -->
                        <table class="table">
                            <thead style="
                                color: #7B8CB6 !important;
                            ">
                                <tr style="text-align: center" id="table-header" data-filter-value="0"
                                    data-filter="none" data-ignore-first="1">
                                    <!--  -->
                                    <th style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer">
                                            </span>
                                        </div>
                                    </th>
                                    <!--id-->
                                    <th data-filter-value="0" data-filter="user_id"
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="user_id">ID
                                            </span>
                                            <icon data-filter="user_id"
                                                class="material-icons material-symbols-outlined invisible" style="
                                            cursor: pointer;
                                            
                                        ">
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    <th data-filter-value="0" data-filter="ciu_number"
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="ciu_number">Phone Number</span>
                                            <icon data-filter="ciu_number"
                                                class="material-icons material-symbols-outlined invisible" style="
                                                cursor: pointer;
                                                
                                            ">
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    <th data-filter-value="0" data-filter="user_name"
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="user_name">User Name</span>
                                            <icon data-filter="user_name"
                                                class="material-icons material-symbols-outlined invisible" style="
                                                cursor: pointer;
                                                
                                            ">
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    
                                    <!-- Payment method -->
                                    <th data-filter-value="0" 
                                        style="color: rgb(128, 128, 128) !important"
                                        data-filter="payment_method">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="payment_method">Payment method
                                            </span>
                                            <icon data-filter="payment_method"
                                                class="material-icons material-symbols-outlined invisible" style="cursor: pointer" >
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    <!-- Upi Id -->
                                    <th data-filter-value="0"
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span >UPI Id
                                            </span>
                                            <icon
                                                class="material-icons material-symbols-outlined invisible" >
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    <!-- Is Upi Valid -->
                                    <th data-filter-value="0" 
                                        style="color: rgb(128, 128, 128) !important"
                                        data-filter="is_upi_valid">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="is_upi_valid">Upi Valid
                                            </span>
                                            <icon data-filter="is_upi_valid"
                                                class="material-icons material-symbols-outlined invisible" style="cursor: pointer" >
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    <!-- Upi Valid Name-->
                                    <th data-filter-value="0" 
                                        style="color: rgb(128, 128, 128) !important"
                                        data-filter="upi_valid_name">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="upi_valid_name">Valid Upi Name
                                            </span>
                                            <icon data-filter="upi_valid_name"
                                                class="material-icons material-symbols-outlined invisible" style="cursor: pointer" >
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    <!-- Is Bank Valid -->
                                    <th data-filter-value="0" 
                                        style="color: rgb(128, 128, 128) !important"
                                        data-filter="is_bank_valid">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="is_bank_valid">Bank Valid
                                            </span>
                                            <icon data-filter="is_bank_valid"
                                                class="material-icons material-symbols-outlined invisible" style="cursor: pointer" >
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    <!-- Bank Valid Name-->
                                    <th data-filter-value="0" 
                                        style="color: rgb(128, 128, 128) !important"
                                        data-filter="bank_valid_name">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="bank_valid_name">Valid Bank Name
                                            </span>
                                            <icon data-filter="bank_valid_name"
                                                class="material-icons material-symbols-outlined invisible" style="cursor: pointer" >
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    <!-- Cashback Amount -->
                                    <th 
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span  >Cashback Amount
                                            </span>
                                            <icon 
                                                class="material-icons material-symbols-outlined invisible">
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    <!-- Referral Amount -->
                                    <th 
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span  >Referral
                                                Amount
                                            </span>
                                            <icon 
                                                class="material-icons material-symbols-outlined invisible"">
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    <!-- Total Amount -->
                                    <th 
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span  >Total
                                                Amount
                                            </span>
                                            <icon 
                                                class="material-icons material-symbols-outlined invisible"">
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                </tr>
                            </thead>

                            <tr id="table-filter-row"
                                style="border-bottom: 1px solid rgb(199, 199, 199) ; background: #e4ebfd;">
                                <th>
                                    <input class="form-check-input" type="checkbox" value="" id="allCheck"
                                        style=" width: 20px; height: 20px;">
                                </th>
                                <th>
                                    <input type="text" name="user_id" class="form-control" 
                                        id="user_id" />
                                </th>
                                <th>
                                    <input type="text" name="ciu_number" class="form-control" 
                                        id="ciu_number" />
                                </th>
                                <th>
                                    <input type="text" name="user_name" class="form-control" 
                                        id="user_name" />
                                </th>
                                
                                <th>
                                    <select name="payment_method" id="payment_method" class="form-control">
                                        <option value="">Select</option>
                                        <option value="upi">upi</option>
                                        <option value="bank">bank</option>
                                    </select>
                                </th>
                                <th></th>
                                <th>
                                    <select name="is_upi_valid" id="is_upi_valid" class="form-control">
                                        <option value="">Select</option>
                                        <option value="true">true</option>
                                        <option value="false">false</option>
                                    </select>
                                </th>
                                <th>
                                    <input type="text" name="upi_valid_name" class="form-control" 
                                        id="upi_valid_name" />
                                </th>
                                <th>
                                    <select name="is_bank_valid" id="is_bank_valid" class="form-control">
                                        <option value="">Select</option>
                                        <option value="true">true</option>
                                        <option value="false">false</option>
                                    </select>
                                </th>
                                <th>
                                    <input type="text" name="bank_valid_name" class="form-control" 
                                        id="bank_valid_name" />
                                </th>
                                <th>
                                    <!-- <input type="text" name="cashback_claimed" class="form-control" style="width:100px;"
                                        id="cashback_claimed" /> -->
                                </th>
                                <th>
                                    <!-- <input type="text" name="cashback_claimed" class="form-control" style="width:100px;"
                                        id="cashback_claimed" /> -->
                                </th>
                                <th>
                                    <!-- <input type="text" name="cashback_claimed" class="form-control" style="width:100px;"
                                        id="cashback_claimed" /> -->
                                </th>
                                <th>
                                    <div><button style="margin: 0;" class="ci-btn" id="payAll" disabled>Process</button>
                                    </div>
                                </th>
                            </tr>
                            <tbody id="data-to-show">
                                <tr>
                                    <td colspan="10">
                                        <div class="d-flex justify-content-center align-items-center">
                                            <div class="sbl-circ-path"></div>
                                        </div>
                                    </td>
                                </tr>
                            </tbody>
                            <tfoot>
                                <tr>
                                    <td colspan="10" style="
                                        text-align: center;
                                        color: black !important;
                                    ">
                                        <div
                                            class="display: flex; align-items: center; justify-content: center; flex-direction: column;">
                                            <%# prevPageBtn %>
                                                <span class="material-icons material-symbols-outlined" style="
                                                position: relative;
                                                top: 4px;
                                                right: 8px;
                                                font-size: 18px;
                                                color: white;
                                                background: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                                padding: 2px;
                                                border-radius: 50%;
                                                cursor: pointer;
                                            " id="prevPage">
                                                    west
                                                </span>
                                                <%# total pages label %>
                                                    <label for="pageNo" style="
                                                margin-right: 10px;
                                                color: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                            ">Page No:</label>
                                                    <%# input %>
                                                        <input type="number" style="
                                                width: 50px;
                                                display: inline;
                                                height: 20px;
                                                border-radius: 0 !important;
                                                padding-right: 0 !important;
                                                padding-left: 2px !important;
                                            " id="page-no" name="pageNo" class="form-control" max="50" min="1"
                                                            value="1" />
                                                        <%# /50 %>

                                                            <span style="
                                                margin-left: 10px;
                                                color: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                            " id="pages">/ 50</span>
                                                            <%# no of entries %>

                                                                <span style="
                                                margin-left: 10px;
                                                color: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                            " id="entries">(2000 entries)</span>

                                                                <span class="material-icons material-symbols-outlined"
                                                                    style="
                                                position: relative;
                                                top: 4px;
                                                left: 8px;
                                                font-size: 18px;
                                                color: white;
                                                background: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                                padding: 2px;
                                                border-radius: 50%;
                                                cursor: pointer;
                                            " id="nextPage">
                                                                    east
                                                                </span>
                                        </div>
                                    </td>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
                <!-- Javascripts -->
                <script src="./admin/assets/plugins/jquery/jquery-3.5.1.min.js"></script>
                <script src="./admin/assets/plugins/bootstrap/js/popper.min.js"></script>
                <script src="./admin/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
                <script src="./admin/assets/plugins/perfectscroll/perfect-scrollbar.min.js"></script>
                <script src="./admin/assets/plugins/pace/pace.min.js"></script>
                <script src="./admin/assets/plugins/highlight/highlight.pack.js"></script>
                <script src="./admin/assets/js/main.min.js"></script>
                <script src="./admin/assets/js/custom.js"></script>
                <script src="./admin/assets/plugins/flatpickr/flatpickr.js"></script>
                <script src="./admin/assets/js/utils/blockModalsEsacping.js"></script>
                <script src="./admin/assets/js/common.js"></script>
                <!-- <script src="./admin/assets/js/approvedApplications.js"></script> -->
                <script src="/./admin/assets/js/claimCashbackRequests.js"></script>
</body>

</html>