<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../common/commonHeader'); %>
        <link href="./admin/assets/plugins/summernote/summernote-lite.min.css" rel="stylesheet">
</head>

<body>
<!--  -->


    <div class="app align-content-stretch d-flex flex-wrap">
      <%- include('../common/commonSidebar' , {sidebarData : sidebarDataByServer}); %>
            <%- include('../common/commonModals'); %>
                <div class="app-container">
                    <div class="app-content">
                        <div class="content-wrapper">
                            <div class="container-fluid">
                                <div class="row">
                                    <div class="col">
                                        <div class="app-content">
                                            <h1 class="application-title  bold">
                                                New Brand
                                            </h1>
                                            <div class="top-btns">
                                                <button class="ci-btn ci-btn-green" id="save-brand" disabled  >Save</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="card bg-card">
                                        <form >
                                            <div class="ci-card-body newoffer card-body">
                                                <div class="col-8 shadowBox p-4" id="left-form">
                                                    <div class="row">
                                                        <div class="col-6">
                                                            <div class="row">
                                                                <label for="BrandName" class="form-label ">Brand Name<span class="required">*</span></label>
                                                                <input type="text" class="form-control" id="BrandName" name="BrandName" required>
                                                            </div>
                                                            <div class="row mt-3">
                                                                <label for="BrandWebsite" class="form-label ">Brand Website</label>
                                                                <input type="text" class="form-control" id="BrandWebsite" name="BrandWebsite" required>
                                                            </div>
                                                            <div class="row px-0 mt-3">
                                                                <div class="col-6">
                                                                    <label for="BrandAlexa" class="form-label ">Brand Alexa</label>
                                                                    <input type="number" class="form-control" id="BrandAlexa" name="BrandAlexa" required>
                                                                </div>
                                                                <div class="col-6"  >
                                                                    <div class="d-flex flex-column justify-content-center align-items-center ">
                                                                        <label for="URLLauncher_check" class="form-label ">URL Launcher Check</label>
                                                                        <label class="switch">
                                                                            <input type="checkbox" id="URLLauncher_check">
                                                                            <span class="slider round"></span>
                                                                        </label>
                                                                    </div>
                                                                </div>
                                                        
                                                            </div>
                                                        </div>
                                                        <div class="col-6" style="display: flex; flex-direction: column; justify-content: flex-start; padding: 0 20px ; ">
                                                            <label for="brandLogoImg" class="form-label ">Brand Logo</label>
                                                            <div class="imageUploadBox form-input ">
                                                                <div class="preview">
                                                                    <img id="brandLogo-preview" class="previewImg">
                                                                </div>
                                                               <i class="material-icons has-sub-menu action-btns deleteImage" >close</i>
                                                                <label for="brandLogo" class="">Choose</label>
                                                                <input type="file" id="brandLogo" name="brandLogo" accept="image/*" onchange="showPreview(event);">
                                                            </div>

                                                        </div>
                                                    </div>
                                                    <div class="row ">
                                                        <div class="col-6" style="display: flex; flex-direction: column; justify-content: center; padding: 20px">
                                                            <label for="brandImg" class="form-label ">Brand Image</label>
                                                            <div class="imageUploadBox form-input ">
                                                                <div class="preview">
                                                                    <img id="brandImg-preview" class="previewImg">
                                                                </div>
                                                               <i class="material-icons has-sub-menu action-btns deleteImage" >close</i>
                                                                <label for="brandImg" class="">Choose</label>
                                                                <input type="file" id="brandImg" accept="image/*" onchange="showPreview(event);">
                                                            </div>
                                                        </div>
                                                        <div class="col-6" style="display: flex; flex-direction: column; justify-content: center; padding: 20px">
                                                            <label for="brandLogoBigImg" class="form-label ">Brand Big Logo</label>
                                                            <div class="imageUploadBox form-input ">
                                                                <div class="preview">
                                                                    <img id="brandLogoBig-preview" class="previewImg">
                                                                </div>
                                                               <i class="material-icons has-sub-menu action-btns deleteImage" >close</i>
                                                                <label for="brandLogoBig" class="">Choose</label>
                                                                <input type="file" id="brandLogoBig" accept="image/*" onchange="showPreview(event);">
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>
                                                <div class="col-4 " style="padding-left: 28px!important;">
                                                    <div class="col-md-12 ">
                                                        <div class=" form-group d-flex flex-column gap-3  " id="multiSelectWrapper">
                                                            <div class="requiredWrapper">
                                                                <div class=""><span class="required">*</span> <span>Required</span></div>
                                                                <!-- <div class=""><span class="oneofrequired">**</span><span>One of the marked is required</span></div> -->
                                                            </div>
                                                            <div class="selectBox" id="offersSelectBox" >
                                                                <label 
                                                                    class="multiInputLabel"
                                                                    id="offersLabel"
                                                                >
                                                                    Offers (0)
                                                                </label>
                                                                <input
                                                                    type="text"
                                                                    name="offersInput"
                                                                    id="offersInput"
                                                                    class="multiSelectInput form-control"
                                                                    placeholder="Select an Option"
                                                                    autocomplete="off"
                                                                />
                                                                <button
                                                                    class="material-icons material-symbols-outlined input-expand-icon"
                                                                    id="offersDropdownIcon"
                                                                >
                                                                    expand_more
                                                                </button>
                                                                <div
                                                                    id="offersDropdown"
                                                                    class="selectBoxDropdown dropdownHidden "
                                                                ></div>
                                                                <div class="selectBoxSelected" id="offersSelectBoxSelected"></div>
                                                            </div>
                                                        </div>
                                                       
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Javascripts -->
                <script src="./admin/assets/plugins/jquery/jquery-3.5.1.min.js"></script>
                <script src="./admin/assets/plugins/bootstrap/js/popper.min.js"></script>
                <script src="./admin/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
                <script src="./admin/assets/plugins/perfectscroll/perfect-scrollbar.min.js"></script>
                <script src="./admin/assets/plugins/pace/pace.min.js"></script>
                <script src="./admin/assets/plugins/highlight/highlight.pack.js"></script>
                <script src="./admin/assets/plugins/summernote/summernote-lite.min.js"></script>
                <script src="./admin/assets/js/main.min.js"></script>
                <script src="./admin/assets/js/custom.js"></script>
                <script src="./admin/assets/js/utils/blockModalsEsacping.js"></script>
                <script src="./admin/assets/js/utils/multiUniSelectObject.js"></script>
                <script src="./admin/assets/js/commonNew.js"></script>
                <script src="./admin/assets/js/newBrand.js"></script>

</body>

</html>




