                            </tr>
                        </tbody>
                        <tfoot>
                            <tr>
                                <td
                                    colspan="10"
                                    style="
                                        text-align: center;
                                        color: black !important;
                                    "
                                >
                                    <div
                                        class="display: flex; align-items: center; justify-content: center; flex-direction: column;"
                                    >
                                    <%# prevPageBtn %>
                                        <span
                                            class="material-icons material-symbols-outlined"
                                            style="
                                                position: relative;
                                                top: 4px;
                                                right: 8px;
                                                font-size: 18px;
                                                color: white;
                                                background: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                                padding: 2px;
                                                border-radius: 50%;
                                                cursor: pointer;
                                            "
                                            id="prevPage"
                                        >
                                            west
                                        </span>
                                    <%# total pages label %>
                                        <label
                                            for="pageNo"
                                            style="
                                                margin-right: 10px;
                                                color: rgb(
                                                    128,
                                                    128,
