<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../common/commonHeader'); %>
</head>

<body>
<!--  -->


    <div class="app align-content-stretch d-flex flex-wrap">
      <%- include('../common/commonSidebar' , {sidebarData : sidebarDataByServer}); %>
            <%- include('../common/commonModals'); %>
                <div class="app-container">
                    <div class="app-content">
                        <div class="content-wrapper">
                            <div class="container-fluid">
                                <div class="row">
                                    <div class="col">
                                        <div class="app-content">
                                            <h1 class="application-title  bold">
                                                New Lounge
                                            </h1>
                                            <div class="top-btns">
                                                <button class="ci-btn ci-btn-green" id="save-lounge" disabled  data-bs-toggle="modal" data-bs-target="#saveModal">Save</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="card bg-card">
                                        <form >
                                            <div class="ci-card-body newoffer card-body">
                                                <div class="col-8 shadowBox p-4" id="left-form">
                                                    <div class="row">
                                                        <div class="col-12">
                                                            <label for="LoungeName" class="form-label ">Lounge Name<span class="required">*</span></label>
                                                                <input type="text" class="form-control" id="LoungeName" name="LoungeName" required>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-4">
                                                         <div class="col-6">
                                                             <label for="LoungeNamePublic" class="form-label ">Lounge Name Public<span class="required">*</span></label>
                                                                <input type="text" class="form-control" id="LoungeNamePublic" name="LoungeNamePublic" required>
                                                        </div>
                                                        <div class="col-6">
                                                            <label for="Location" class="form-label ">Lounge Location<span class="required">*</span></label>
                                                                <input type="text" class="form-control" id="Location" name="Location" required>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-4">
                                                        
                                                                <div class="col-6">
                                                                    <label for="TerminalsSelect" class="form-label ">Lounge Terminal<span class="required">*</span></label>
                                                                        <select class="form-control form-select" id="TerminalsSelect" name="TerminalsSelect">
                                                                            <option value="">Select Terminal</option>
                                                                            <option value="DomesticTerminal">Domestic Terminal</option>
                                                                            <option value="InternationalTerminal">International Terminal</option>
                                                                            <option value="Terminal1">Terminal 1</option>
                                                                            <option value="Terminal2">Terminal 2</option>
                                                                            <option value="Terminal3">Terminal 3</option>
                                                                            </select>
                                                                </div>
                                                                <div class="col-6 ">
                                                                    <label for="Lounge Timings" class="form-label gap-4" id="loungeTimings">
                                                                        <p>Lounge Timings<span class="required">*</span></p>
                                                                        <button id="hrsBtn">24 hrs</button>
                                                                    </label>
                                                                    <input type="text" class="form-control"
                                                                    id="LoungeTiming"
                                                                    name="LoungeTiming"
                                                                    placeholder="XX:XX-XX:XX 24hrs format">     
                                                                </div>
                                                       
                                                    </div>
                                                    <div class="row mt-4">
                                                        <div class="col-12">
                                                            <label for="Directions" class="form-label ">Directions<span class="required">*</span></label>
                                                            <textarea type="text" class="form-control form-textarea"  id="Directions" name="Directions" required> </textarea>
                                                        </div>
                                                        <div class="col-12 mt-3">
                                                            <label for="Introduction" class="form-label ">Introduction</label>
                                                            <textarea type="text" class="form-control form-textarea"  id="Introduction" name="Introduction" required> </textarea>
                                                        </div>
                                                        <div class="col-12 mt-3">
                                                            <label for="MainSummary" class="form-label ">Main Summary</label>
                                                            <textarea type="text" class="form-control form-textarea"  id="MainSummary" name="MainSummary" required> </textarea>
                                                        </div>
                                                         <div class="col-12 mt-3">
                                                            <label for="ImportantInformation" class="form-label ">Important Information</label>
                                                            <textarea type="text" class="form-control form-textarea"  id="ImportantInformation" name="ImportantInformation" required> </textarea>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-4">
                                                        
                                                    </div>
                                                    <div class="row mt-4">
                                                        <div class="col-12">
                                                            <label for="Amenties" class="form-label ">Amenties<span class="required">*</span></label>
                                                            <div class="selectBox container">
                                                                <div class="row">
                                                                    <div class="col">
                                                                        <div class="d-flex flex-column justify-content-center align-items-center ">
                                                                            <label for="Food" class="form-label ">Food</label>
                                                                            <label class="switch">
                                                                                <input type="checkbox" id="Food">
                                                                                <span class="slider round"></span>
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col">
                                                                        <div class="d-flex flex-column justify-content-center align-items-center ">
                                                                            <label for="DisabledAccess" class="form-label ">Disabled Access</label>
                                                                            <label class="switch">
                                                                                <input type="checkbox" id="DisabledAccess">
                                                                                <span class="slider round"></span>
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col">
                                                                        <div class="d-flex flex-column justify-content-center align-items-center ">
                                                                            <label for="TV" class="form-label ">TV</label>
                                                                            <label class="switch">
                                                                                <input type="checkbox" id="TV">
                                                                                <span class="slider round"></span>
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col">
                                                                        <div class="d-flex flex-column justify-content-center align-items-center ">
                                                                            <label for="SmokingArea" class="form-label ">Smoking Area</label>
                                                                            <label class="switch">
                                                                                <input type="checkbox" id="SmokingArea">
                                                                                <span class="slider round"></span>
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col">
                                                                        <div class="d-flex flex-column justify-content-center align-items-center ">
                                                                            <label for="AirConditioning" class="form-label ">Air Conditioning</label>
                                                                            <label class="switch">
                                                                                <input type="checkbox" id="AirConditioning">
                                                                                <span class="slider round"></span>
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="row mt-4">
                                                                    <div class="col">
                                                                        <div class="d-flex flex-column justify-content-center align-items-center ">
                                                                            <label for="NoSmoking" class="form-label ">No Smoking</label>
                                                                            <label class="switch">
                                                                                <input type="checkbox" id="NoSmoking">
                                                                                <span class="slider round"></span>
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col">
                                                                        <div class="d-flex flex-column justify-content-center align-items-center ">
                                                                            <label for="AlcoholicDrinks" class="form-label ">Alcoholic Drinks</label>
                                                                            <label class="switch">
                                                                                <input type="checkbox" id="AlcoholicDrinks">
                                                                                <span class="slider round"></span>
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col">
                                                                        <div class="d-flex flex-column justify-content-center align-items-center ">
                                                                            <label for="NewspapersMagazines" class="form-label ">Newspapers</label>
                                                                            <label class="switch">
                                                                                <input type="checkbox" id="NewspapersMagazines">
                                                                                <span class="slider round"></span>
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col">
                                                                        <div class="d-flex flex-column justify-content-center align-items-center ">
                                                                            <label for="WiFi" class="form-label ">WiFi</label>
                                                                            <label class="switch">
                                                                                <input type="checkbox" id="WiFi">
                                                                                <span class="slider round"></span>
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                     <div class="col">
                                                                        <div class="d-flex flex-column justify-content-center align-items-center ">
                                                                            <label for="Television" class="form-label ">Television</label>
                                                                            <label class="switch">
                                                                                <input type="checkbox" id="Television">
                                                                                <span class="slider round"></span>
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-4">
                                                         <div class="col-6" style="display: flex; flex-direction: column; justify-content: flex-start; padding: 0 20px ; ">
                                                            <label for="loungeImage" class="form-label ">Lounge Image</label>
                                                            <div class="imageUploadBox form-input ">
                                                                <div class="preview">
                                                                    <img id="lounge_image-preview" class="previewImg">
                                                                </div>
                                                               <i class="material-icons has-sub-menu action-btns deleteImage" >close</i>
                                                                <label for="lounge_image" class="">Choose</label>
                                                                <input type="file" id="lounge_image" accept="image/*" onchange="showPreview(event);">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                
                                                <div class="col-4 " style="padding-left: 28px!important;">
                                                    <div class="col-md-12 ">
                                                       <div class="requiredWrapper">
                                                            <div class=""><span class="required">*</span> <span>Required</span></div>
                                                            <!-- <div class=""><span class="oneofrequired">**</span><span>One of the marked is required</span></div> -->
                                                            
                                                        </div>

                                                       
                                                        <div class=" form-group d-flex flex-column gap-3 mt-3 " id="multiSelectWrapper">
                                                            <div class="selectBox" id="airportsSelectBox" >
                                                                <label 
                                                                    class="multiInputLabel"
                                                                    id="airportsLabel"
                                                                >
                                                                    Airport 
                                                                </label>
                                                                <span class="required">*</span>
                                                                <input
                                                                    type="text"
                                                                    name="airportsInput"
                                                                    id="airportsInput"
                                                                    class="multiSelectInput form-control"
                                                                    placeholder="Select an Airport"
                                                                    autocomplete="off"
                                                                />
                                                                <button
                                                                    class="material-icons material-symbols-outlined input-expand-icon"
                                                                    id="airportsDropdownIcon"
                                                                >
                                                                    expand_more
                                                                </button>
                                                                <div
                                                                    id="airportsDropdown"
                                                                    class="selectBoxDropdown dropdownHidden "
                                                                ></div>
                                                                <div class="selectBoxSelected" id="airportsSelectBoxSelected"></div>
                                                            </div>
                                                            <div class="selectBox" id="creditCardsSelectBox" >
                                                                <label 
                                                                    class="multiInputLabel"
                                                                    id="creditCardsLabel"
                                                                >
                                                                    Credit Cards (0)
                                                                </label>
                                                                <!-- <span class="required">*</span> -->
                                                                <input
                                                                    type="text"
                                                                    name="creditCardsInput"
                                                                    id="creditCardsInput"
                                                                    class="multiSelectInput form-control"
                                                                    placeholder="Select Options"
                                                                    autocomplete="off"
                                                                />
                                                                <button
                                                                    class="material-icons material-symbols-outlined input-expand-icon"
                                                                    id="creditCardsDropdownIcon"
                                                                >
                                                                    expand_more
                                                                </button>
                                                                <div
                                                                    id="creditCardsDropdown"
                                                                    class="selectBoxDropdown dropdownHidden "
                                                                ></div>
                                                                <div class="selectBoxSelected" id="creditCardsSelectBoxSelected"></div>
                                                            </div>
                                                            <div class="selectBox" id="loungeNetworksSelectBox" >
                                                                <label 
                                                                    class="multiInputLabel"
                                                                    id="loungeNetworksLabel"
                                                                >
                                                                    Lounge Networks (0)
                                                                </label>
                                                                <!-- <span class="required">*</span> -->
                                                                <input
                                                                    type="text"
                                                                    name="loungeNetworksInput"
                                                                    id="loungeNetworksInput"
                                                                    class="multiSelectInput form-control"
                                                                    placeholder="Select Options"
                                                                    autocomplete="off"
                                                                />
                                                                <button
                                                                    class="material-icons material-symbols-outlined input-expand-icon"
                                                                    id="loungeNetworksDropdownIcon"
                                                                >
                                                                    expand_more
                                                                </button>
                                                                <div
                                                                    id="loungeNetworksDropdown"
                                                                    class="selectBoxDropdown dropdownHidden "
                                                                ></div>
                                                                <div class="selectBoxSelected" id="loungeNetworksSelectBoxSelected"></div>
                                                            </div>
                                                        </div>
                                                       
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Javascripts -->
                <script src="./admin/assets/plugins/jquery/jquery-3.5.1.min.js"></script>
                <script src="./admin/assets/plugins/bootstrap/js/popper.min.js"></script>
                <script src="./admin/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
                <script src="./admin/assets/plugins/perfectscroll/perfect-scrollbar.min.js"></script>
                <script src="./admin/assets/plugins/pace/pace.min.js"></script>
                <script src="./admin/assets/plugins/highlight/highlight.pack.js"></script>
                <script src="./admin/assets/js/main.min.js"></script>
                <script src="./admin/assets/js/custom.js"></script>
                <script src="./admin/assets/js/utils/blockModalsEsacping.js"></script>
<script src="./admin/assets/js/utils/multiUniSelectObject.js"></script>
                <script src="./admin/assets/js/commonNew.js"></script>
                <script src="./admin/assets/js/newLounge.js"></script>

</body>

</html>