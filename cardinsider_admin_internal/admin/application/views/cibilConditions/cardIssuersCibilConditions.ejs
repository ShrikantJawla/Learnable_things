<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('../common/commonHeader'); %>
        <link
            href="./admin/assets/plugins/datatables/datatables.min.css"
            rel="stylesheet"
        />
    </head>

    <body>
        <div class="app align-content-stretch d-flex flex-wrap">
          <%- include('../common/commonSidebar' , {sidebarData : sidebarDataByServer}); %>
            <div class="app-container">
                <div class="app-content">
                    <div class="content-wrapper">
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col">
                                    <div class="app-content">
                                        <h1 class="application-title">Card Issuers Conditions</h1>
                                        <hr />
                                    </div>
                                    <button class="btn btn-primary" onclick="window.open('/card-issuers-cibil-conditions-add-new','_blank')">
                                        Add Item
                                    </button>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col">
                                    <div class="card">
                                        <div class="card-header">
                                            <h5 class="card-title">Basic</h5>
                                        </div>
                                        <div class="card-body">

                                            <table id="datatable1" class="display" style="width:100%">
                                            <!-- id,method,account_name,account_number,bank_name,ifsc_code,upi_id,card_insider_user,published_at,created_by,updated_by,created_at,updated_at,ciu_id,user_name -->
                                                    <thead>
                                                        <tr>
                                                            <th>ID</th>
                                                            <th>Issuer Name</th>
                                                            <th>Score</th>
                                                            <th>Min age</th>
                                                            <th>Mex age</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody id="js-append-offer-rows">
                                                        <% for(let i = 0; i < allRecord.length; i++){ %>
                                                            <tr>
                                                                <td>
                                                                    <%=allRecord[i]['iwcc_id']%>
                                                                </td>
                                                                <td>
                                                                    <%=allRecord[i]['IssuerName']%>
                                                                </td>
                                                                <td>
                                                                    <%=allRecord[i]['iwcc_min_cibil']%>
                                                                </td>
                                                                <td>
                                                                    <%=allRecord[i]['iwcc_min_age']%>
                                                                </td>
                                                                <td>
                                                                    <%=allRecord[i]['iwcc_max_age']%>
                                                                </td>
                                                            </tr>
                                                            <% }%>
                                                    </tbody>
                                                  
                                            </table>

                                        </div>

                                    </div>
                                </div>
                            </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Javascripts -->
        <script src="./admin/assets/plugins/jquery/jquery-3.5.1.min.js"></script>
        <script src="./admin/assets/plugins/bootstrap/js/popper.min.js"></script>
        <script src="./admin/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
        <script src="./admin/assets/plugins/perfectscroll/perfect-scrollbar.min.js"></script>
        <script src="./admin/assets/plugins/pace/pace.min.js"></script>
        <script src="./admin/assets/plugins/highlight/highlight.pack.js"></script>
        <script src="./admin/assets/plugins/datatables/datatables.min.js"></script>
        <script src="./admin/assets/js/main.min.js"></script>
        <script src="./admin/assets/js/custom.js"></script>
        <script src="./admin/assets/js/pages/datatables.js"></script>
    </body>
</html>
