<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('../common/commonHeader'); %>
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css" integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g==" crossorigin="anonymous" referrerpolicy="no-referrer" />
        <link
            href="./admin/assets/plugins/flatpickr/flatpickr.min.css"
            rel="stylesheet"
        />
    </head>

    <body>
       <div class="app align-content-stretch d-flex flex-wrap">
          <%- include('../common/commonSidebar' , {sidebarData : sidebarDataByServer}); %>
            <%- include('../common/commonModals'); %>
            <div class="app-container ci-table-padding">
                <div class="d-flex flex-column">
                    <div class="app-content">
                        <h1 class="application-title pb-1">
                            Card Insider Users
                        </h1>
                    </div>
                    <div
                        style="
                            display: flex;
                            margin-bottom: 10px;
                            gap: 10px;
                            margin-left: auto;
                        "
                    >
                        <button
                            class="btn btn-success"
                            id="filterReset"
                        >
                            Reset Filters
                        </button>
                        <button class="btn btn-primary"  onclick="window.location.href = '/airports-list-new'" >
                            Add Item
                        </button>
                        <button class="btn btn-info">Export csv</button>
                    </div>
                </div>
                <div class="card ci-scrollX">
                    <header class="card-header d-flex justify-content-between" >
                        <div
                            class="d-flex align-items-center"
                            style="margin-bottom: 20px; gap: 12px"
                        >
                            <select
                                name="entriesPerPage"
                                id="entriesPerPage"
                            >
                                 <option value="10">10</option>
                                <option value="25">25</option>
                                <option value="50">50</option>
                                <option value="100">100</option>
                                <option value="200">200</option>
                            </select>
                            <label
                                for="entriesPerPage"
                                style="color: rgb(128, 128, 128)"
                            >
                                entries / page</label
                            >
                        </div>
                        <icon class="material-icons material-symbols-outlined" style="cursor: pointer;"  data-bs-toggle="collapse" href="#table-filter-row" role="button"  >
                            filter_list
                        </icon>
                        
                    </header>
                    <!-- <div class="collapse" id="collapseExample">
                            <div class="card card-body ">
                                <div class="filterBox">
                                    <div class="contains">
                                    </div>

                                    <div class="dateRange">
                                        
                                    </div>
                                    <div class="selectField">
                                       
                                    </div>

                                </div>
                            </div>
                        </div> -->
                    <table class="table">
                        <thead
                            style="
                                color: rgb(
                                    128,
                                    128,
                                    128
                                ) !important;
                            "
                        >
                        <tr
                                style="text-align: center"
                                id="table-header"
                                data-filter-value="0"
                                data-filter="none"
                            >
                            <!--id-->
                            <th
                                    data-filter-value="0"
                                    data-filter="id"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                <div class="d-flex ">
                                    <span
                                            style="cursor: pointer"
                                            data-filter="id"
                                            >ID
                                    </span>
                                    <icon
                                        data-filter="id"
                                        class="material-icons material-symbols-outlined invisible"
                                        style="
                                            cursor: pointer;
                                            
                                        "
                                    >
                                        straight
                                    </icon>
                                </div>
                            </th>
                             <!--params_id-->
                             <th
                             data-filter-value="0"
                             data-filter="params_id"
                             style="color: rgb(128, 128, 128) !important"
                         >
                         <div class="d-flex ">
                             <span
                                     style="cursor: pointer"
                                     data-filter="params_id"
                                     >Params Id
                             </span>
                             <icon
                                 data-filter="params_id"
                                 class="material-icons material-symbols-outlined invisible"
                                 style="
                                     cursor: pointer;
                                     
                                 "
                             >
                                 straight
                             </icon>
                         </div>
                     </th>
                            <!-- ciu_first_name -->
                            <th
                                    data-filter-value="0"
                                    data-filter="ciu_first_name"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                <div class="d-flex ">
                                    <span
                                            style="cursor: pointer"
                                            data-filter="ciu_first_name"
                                            >First Name</span
                                        >
                                        <icon
                                            data-filter="ciu_first_name"
                                            class="material-icons material-symbols-outlined invisible"
                                            style="
                                                cursor: pointer;
                                                
                                            "
                                        >
                                            straight
                                        </icon>
                                </div>
                            </th>                          
                            <!-- ciu_last_name -->
                            <th
                                    data-filter-value="0"
                                    data-filter="ciu_last_name"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                <div class="d-flex ">
                                    <span
                                            style="cursor: pointer"
                                            data-filter="ciu_last_name"
                                            >Last Name
                                    </span >
                                    <icon
                                        data-filter="ciu_last_name"
                                        class="material-icons material-symbols-outlined invisible"
                                        style="
                                            cursor: pointer;
                                            
                                        "
                                    >
                                        straight
                                    </icon>
                                </div>
                            </th>
                            <!-- ciu_email -->
                            <th
                                        data-filter-value="0"
                                        data-filter="ciu_email"
                                        style="color: rgb(128, 128, 128) !important"
                                    >
                                    <div class="d-flex ">
                                        <span
                                                style="cursor: pointer"
                                                data-filter="ciu_email"
                                                >Email
                                        </span>
                                        <icon
                                            data-filter="ciu_email"
                                            class="material-icons material-symbols-outlined invisible"
                                            style="
                                                cursor: pointer;
                                                
                                            "
                                        >
                                            straight
                                        </icon>
                                    </div>
                            </th>
                            <!-- ciu_number -->
                            <th
                                        data-filter-value="0"
                                        data-filter="ciu_number"
                                        style="color: rgb(128, 128, 128) !important"
                                    >
                                    <div class="d-flex ">
                                        <span
                                                style="cursor: pointer"
                                                data-filter="ciu_number"
                                                >Contact Number
                                        </span>
                                        <icon
                                            data-filter="ciu_number"
                                            class="material-icons material-symbols-outlined invisible"
                                            style="
                                                cursor: pointer;
                                                
                                            "
                                        >
                                            straight
                                        </icon>
                                    </div>
                            </th>
                            
                            <!-- ciu_verified -->
                            <th
                                    data-filter-value="0"
                                    data-filter="ciu_verified"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                <div class="d-flex ">
                                    <span
                                            style="cursor: pointer"
                                            data-filter="ciu_verified"
                                            >Verified
                                    </span>
                                    <icon
                                        data-filter="ciu_verified"
                                        class="material-icons material-symbols-outlined invisible"
                                        style="
                                            cursor: pointer;
                                            
                                        "
                                    >
                                        straight
                                    </icon>
                                </div>
                            </th>

                            <!-- created_at -->
                            <th
                                    data-filter-value="0"
                                    data-filter="created_at"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                <div class="d-flex ">
                                    <span
                                            style="cursor: pointer"
                                            data-filter="created_at"
                                            >Created At
                                    </span>
                                    <icon
                                        data-filter="created_at"
                                        class="material-icons material-symbols-outlined invisible"
                                        style="
                                            cursor: pointer;
                                            
                                        "
                                    >
                                        straight
                                    </icon>
                                </div>
                            </th>

                            <!-- updated_at -->
                            <th
                                    data-filter-value="0"
                                    data-filter="updated_at"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                <div class="d-flex ">
                                    <span
                                            style="cursor: pointer"
                                            data-filter="updated_at"
                                            >Updated At
                                    </span>
                                    <icon
                                        data-filter="updated_at"
                                        class="material-icons material-symbols-outlined invisible"
                                        style="
                                            cursor: pointer;
                                            
                                        "
                                    >
                                        straight
                                    </icon>
                                </div>
                            </th>
                            <!-- published_at -->
                            <th
                                    data-filter-value="0"
                                    data-filter="published_at"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                <div class="d-flex ">
                                    <span
                                            style="cursor: pointer"
                                            data-filter="published_at"
                                            >Published At
                                    </span>
                                    <icon
                                        data-filter="published_at"
                                        class="material-icons material-symbols-outlined invisible"
                                        style="
                                            cursor: pointer;
                                            
                                        "
                                    >
                                        straight
                                    </icon>
                                </div>
                            </th>
                            <!-- Actions -->
                            <th
                                    data-filter-value="0"
                                    style="color: rgb(128, 128, 128) !important"
                                >
                                <div class="d-flex ">
                                <span
                                        style="cursor: pointer"
                                        data-filter="Actions"
                                        >Actions</span
                                    >
                            </div>
                            </th>
                        </tr>
                        </thead>
                        <tr id="table-filter-row"  style="border-bottom: 1px solid rgb(199, 199, 199)">
                            <th>
                                <input
                                        type="text"
                                        name="id"
                                        class="form-control"
                                        id="id"
                                    />
                            </th>
                            <th>
                                <input
                                        type="text"
                                        name="params_id"
                                        class="form-control"
                                        id="params_id"
                                    />
                            </th>
                            <th>
                                <input
                                    type="text"
                                    name="ciu_first_name"
                                    class="form-control"
                                    id="ciu_first_name"
                                />
                            </th>
                            <th>
                                <input
                                    type="text"
                                    name="ciu_last_name"
                                    class="form-control"
                                    id="ciu_last_name"
                                />
                            </th>
                            <th>
                                <input
                                    type="text"
                                    name="ciu_email"
                                    class="form-control"
                                    id="ciu_email"
                                />
                            </th>
                            <th>
                                <input
                                    type="text"
                                    name="ciu_number"
                                    class="form-control"
                                    id="ciu_number"
                                />
                            </th>
                            <th>
                                <label class="switch">                                                 
                                    <input type="checkbox" id="ciu_verified" name="ciu_verified">
                                    <span class="slider round"></span>
                                </label>
                            </th>
                            <th>
                                <div class="d-flex" style="margin-bottom: 2px">
                                    <span style="
                                            font-size: 12px;
                                            width: 40px;
                                            color: rgb(
                                                128,
                                                128,
                                                128
                                            ) !important;
                                        ">From</span>
                                    <input type="date" id="from_created_at" name="from_created_at" class="form-control" placeholder="yyyy-mm-dd" style="
                                            width: 90px;
                                            padding: 0;
                                            height: 20px;
                                            border-radius: 3px;
                                            font-size: 11px;
                                        " />
                                </div>

                                <div class="d-flex">
                                    <span style="
                                            font-size: 12px;
                                            width: 40px;
                                            color: rgb(
                                                128,
                                                128,
                                                128
                                            ) !important;
                                        ">To
                                </span>
                                    <input type="date" id="to_created_at" name="to_created_at" class="form-control" style="
                                            width: 90px;
                                            padding: 0;
                                            height: 20px;
                                            border-radius: 3px;
                                            font-size: 11px;
                                        " />
                                </div>
                            </th>
                            <th>
                                <div class="d-flex" style="margin-bottom: 2px">
                                    <span style="
                                            font-size: 12px;
                                            width: 40px;
                                            color: rgb(
                                                128,
                                                128,
                                                128
                                            ) !important;
                                        ">From</span>
                                    <input type="date" id="from_updated_at" name="from_updated_at" class="form-control" placeholder="yyyy-mm-dd" style="
                                            width: 90px;
                                            padding: 0;
                                            height: 20px;
                                            border-radius: 3px;
                                            font-size: 11px;
                                        " />
                                </div>

                                <div class="d-flex">
                                    <span style="
                                            font-size: 12px;
                                            width: 40px;
                                            color: rgb(
                                                128,
                                                128,
                                                128
                                            ) !important;
                                        ">To
                                </span>
                                    <input type="date" id="to_updated_at" name="to_updated_at" class="form-control" style="
                                            width: 90px;
                                            padding: 0;
                                            height: 20px;
                                            border-radius: 3px;
                                            font-size: 11px;
                                        " />
                                </div>
                            </th>
                            <th>
                                <div class="d-flex" style="margin-bottom: 2px">
                                    <select
                                            id="published_at"
                                            name="published_at"
                                            class="form-control"
                                        >
                                            <option value="any">Select Publish Type</option>
                                            <option value="Draft">Draft</option>
                                            <option value="Published">Published</option>
                                    </select>
                                </div>
                            </th>
                            <th></th>
                        </tr>
                        <tbody id="data-to-show">
                            <tr>
                                <td colspan="10">
                                    <div class="d-flex justify-content-center align-items-center">
                                                <div class="sbl-circ-path"></div>
                                    </div>
                                </td>
                            </tr>
                        </tbody>
                        <tfoot>
                            <tr>
                                <td
                                    colspan="10"
                                    style="
                                        text-align: center;
                                        color: black !important;
                                    "
                                >
                                    <div
                                        class="display: flex; align-items: center; justify-content: center; flex-direction: column;"
                                    >
                                    <%# prevPageBtn %>
                                        <span
                                            class="material-icons material-symbols-outlined"
                                            style="
                                                position: relative;
                                                top: 4px;
                                                right: 8px;
                                                font-size: 18px;
                                                color: white;
                                                background: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                                padding: 2px;
                                                border-radius: 50%;
                                                cursor: pointer;
                                            "
                                            id="prevPage"
                                        >
                                            west
                                        </span>
                                    <%# total pages label %>
                                        <label
                                            for="pageNo"
                                            style="
                                                margin-right: 10px;
                                                color: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                            "
                                            >Page No:</label
                                        >
                                    <%# input %>
                                        <input
                                            type="number"
                                            style="
                                                width: 50px;
                                                display: inline;
                                                height: 20px;
                                                border-radius: 0 !important;
                                                padding-right: 0 !important;
                                                padding-left: 2px !important;
                                            "
                                            id="page-no"
                                            name="pageNo"
                                            class="form-control"
                                            max="50"
                                            min="1"
                                            value="1"
                                        />
                                    <%# /50 %>    
                                    
                                        <span
                                            style="
                                                margin-left: 10px;
                                                color: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                            "
                                            id="pages"
                                            >/ 50</span
                                        >
                                    <%# no of entries %>    

                                        <span
                                            style="
                                                margin-left: 10px;
                                                color: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                            "
                                            id="entries"
                                            >(2000 entries)</span
                                        >
                                    
                                        <span
                                            class="material-icons material-symbols-outlined"
                                            style="
                                                position: relative;
                                                top: 4px;
                                                left: 8px;
                                                font-size: 18px;
                                                color: white;
                                                background: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                                padding: 2px;
                                                border-radius: 50%;
                                                cursor: pointer;
                                            "
                                            id="nextPage"
                                        >
                                            east
                                        </span>
                                    </div>
                                </td>
                            </tr>
                        </tfoot>
                    </table>
                </div>
            </div>
        <!-- Javascripts -->
        <script src="./admin/assets/plugins/jquery/jquery-3.5.1.min.js"></script>
        <script src="./admin/assets/plugins/bootstrap/js/popper.min.js"></script>
        <script src="./admin/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
        <script src="./admin/assets/plugins/perfectscroll/perfect-scrollbar.min.js"></script>
        <script src="./admin/assets/plugins/pace/pace.min.js"></script>
        <script src="./admin/assets/plugins/highlight/highlight.pack.js"></script>
        <script src="./admin/assets/plugins/datatables/datatables.min.js"></script>
        <script src="./admin/assets/js/main.min.js"></script>
        <script src="./admin/assets/js/custom.js"></script>
        <script src="./admin/assets/js/pages/datatables.js"></script>
        <script src="./admin/assets/plugins/flatpickr/flatpickr.js"></script>
        <script src="./admin/assets/js/utils/blockModalsEsacping.js"></script><script src="./admin/assets/js/common.js"></script>
        <script src="./admin/assets/js/ciUsers.js"></script>
    </body>
</html>
