<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../common/commonHeader'); %>
        <link href="./admin/assets/plugins/summernote/summernote-lite.min.css" rel="stylesheet">
</head>

<body>
    <!--  -->
  

    <div class="app align-content-stretch d-flex flex-wrap">
        <%- include('../common/commonSidebar',  {sidebarData : sidebarDataByServer}); %>
            <%- include('../common/commonModals'); %>
                <div class="app-container">
                    <div class="app-content">
                        <div class="content-wrapper">
                            <div class="container-fluid">
                                <div class="row">
                                    <div class="col">
                                        <div class="app-content">
                                            <h1 class="application-title  bold">
                                                New Credit Card
                                            </h1>
                                            <div class="top-btns">
                                                <button class="ci-btn ci-btn-green" id="save-creditCard" disabled >Save</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="card bg-card">
                                        <form>
                                            <div class="ci-card-body newoffer card-body">
                                                <div class="col-8 shadowBox p-4" id="left-form">
                                                    <div class="row">
                                                        
                                                        <div class="col-12">
                                                            <label for="CreditCardName" class="form-label">Credit Card Name <span class="required">*</span></label>
                                                            <input type="text" class="form-control" id="CreditCardName" name="CreditCardName" required>
                                                        </div>

                                                    </div>
                                                    <div class="row mt-4">
                                                        <div class="col-12">
                                                            <label for="Highlights" class="form-label" id="Highlights_label">Highlights</label>
                                                            <textarea type="text" class="form-control form-textarea" id="Highlights" name="Highlights" style="resize: vertical; min-height: 160px;" required> </textarea>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-4">
                                                        <div class="col-12">
                                                            <label for="WelcomeOffers" class="form-label" id="WelcomeOffers_label">Welcome Offers </label>
                                                            <textarea type="text" class="form-control form-textarea" id="WelcomeOffers" name="WelcomeOffers" style="resize: vertical; min-height: 160px;" required> </textarea>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-4">

                                                        <div class="col">
                                                            <label for="CardRating" class="form-label">Card Rating <span class="required">*</span></label>
                                                            <input type="number" class="form-control" id="CardRating" name="CardRating" required>
                                                        </div>
                                                        
                                                        <div class="col">
                                                            <label for="JoiningFees" class="form-label">Joining Fees <span class="required">*</span></label>
                                                            <input type="text" class="form-control" id="JoiningFees" name="JoiningFees" required>
                                                        </div>
                                                        <div class="col">
                                                            <label for="RenewalFees" class="form-label">Renewal Fees <span class="required">*</span></label>
                                                            <input type="text" class="form-control" id="RenewalFees" name="RenewalFees" required>
                                                        </div>
                                                        
                                                    </div>
                                                    
                                                    <div class="row mt-4">
                                                        <div class="col-6" style="display: flex; flex-direction: column; justify-content: flex-start; padding: 0 20px ; ">
                                                            <label for="cardImage" class="form-label ">Card Logo</label>
                                                            <div class="imageUploadBox form-input ">
                                                                <div class="preview">
                                                                    <img id="cardimage-preview" class="previewImg">
                                                                </div>
                                                               <i class="material-icons has-sub-menu action-btns deleteImage" >close</i>
                                                                <label for="cardimage" class="">Choose</label>
                                                                <input type="file" id="cardimage" accept="image/*" onchange="showPreview(event);">
                                                            </div>

                                                        </div>
                                                        <div class="col-6">
                                                            <label for="FeeReversal" class="form-label">Renewal Fee Reversal</label>
                                                            <textarea type="text" class="form-control form-textarea" id="FeeReversal" name="FeeReversal" style=" min-height: 240px;" required> </textarea>
                                                        </div>
                                                    </div>
                                                    <div class="row mt-4">
                                                        <label for="BestSuitedFor" class="form-label">Best Suited For <span class="required">*</span></label>
                                                        <div class="selectBox col-12">
                                                            <div class="row">
                                                                <div class="col">
                                                                    <div class="d-flex flex-column align-items-center ">
                                                                        <label for="Movies" class="form-label ">Movies</label>
                                                                        <label class="switch">
                                                                            <input data-parent="BestSuitedFor" type="checkbox" id="Movies">
                                                                            <span class="slider round"></span>
                                                                        </label>
                                                                    </div>
                                                                </div>
                                                                <div class="col">
                                                                    <div class="d-flex flex-column align-items-center ">
                                                                        <label for="Travel" class="form-label ">Travel</label>
                                                                        <label class="switch">
                                                                            <input data-parent="BestSuitedFor" type="checkbox" id="Travel">
                                                                            <span class="slider round"></span>
                                                                        </label>
                                                                    </div>
                                                                </div>
                                                                <div class="col">
                                                                    <div class="d-flex flex-column align-items-center ">
                                                                        <label for="Dining" class="form-label">Dining</label>
                                                                        <label class="switch">
                                                                            <input data-parent="BestSuitedFor" type="checkbox" id="Dining">
                                                                            <span class="slider round"></span>
                                                                        </label>
                                                                    </div>
                                                                </div>
                                                                <div class="col">
                                                                    <div class="d-flex flex-column align-items-center ">
                                                                        <label for="Fuel" class="form-label">Fuel</label>
                                                                        <label class="switch">
                                                                            <input data-parent="BestSuitedFor" type="checkbox" id="Fuel">
                                                                            <span class="slider round"></span>
                                                                        </label>
                                                                    </div>
                                                                </div>
                                                                <div class="col">
                                                                    <div class="d-flex flex-column align-items-center ">
                                                                        <label for="Shopping" class="form-label">Shopping</label>
                                                                        <label class="switch">
                                                                            <input data-parent="BestSuitedFor" type="checkbox" id="Shopping">
                                                                            <span class="slider round"></span>
                                                                        </label>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    
                                                    <label for="Features" class=" mt-4 text-primary ">Features</label>
                                                    <div class="selectBox row"> 
                                                        <div class="col-12">
                                                            <label for="Rewards" class="form-label" id="Rewards_label">Rewards </label>
                                                            <textarea type="text" data-parent="Features" id="RewardsFeatures" class="form-control form-textarea" name="Rewards" style=" min-height: 140px;" required> </textarea>
                                                        </div>
                                                        <div class="col-12 mt-3">
                                                            <label for="Movie" class="form-label">Movie</label>
                                                            <textarea type="text" data-parent="Features" id="MovieFeatures" class="form-control form-textarea" name="Movie" style=" min-height: 140px;" required> </textarea>
                                                        </div>
                                                        <div class="col-12 mt-3">
                                                            <label for="Dining" class="form-label">Dining</label>
                                                            <textarea type="text" data-parent="Features" id="DiningFeatures" class="form-control form-textarea" name="Dining" style=" min-height: 140px;" required> </textarea>
                                                        </div>
                                                        <div class="col-12 mt-3">
                                                            <label for="Travel" class="form-label">Travel</label>
                                                            <textarea type="text" data-parent="Features" id="TravelFeatures" class="form-control form-textarea" name="Travel" style=" min-height: 140px;" required> </textarea>
                                                        </div>
                                                        <div class="col-12 mt-3">
                                                            <label for="Lounge" class="form-label">Lounge</label>
                                                            <textarea type="text" data-parent="Features" id="LoungeFeatures" class="form-control form-textarea" name="Lounge" style=" min-height: 140px;" required> </textarea>
                                                        </div>
                                                        <div class="col-12 mt-3">
                                                            <label for="Golf" class="form-label">Golf</label>
                                                            <textarea type="text" data-parent="Features" id="GolfFeatures" class="form-control form-textarea" name="Golf" style=" min-height: 140px;" required> </textarea>
                                                        </div>
                                                        <div class="col-12 mt-3">
                                                            <label for="Fuel" class="form-label">Fuel</label>
                                                            <textarea type="text" data-parent="Features" id="FuelFeatures" class="form-control form-textarea" name="Fuel" style=" min-height: 140px;" required> </textarea>
                                                        </div>
                                                        <div class="col-12 mt-3">
                                                            <label for="RewardRedemption" class="form-label">Reward Redemption</label>
                                                            <textarea type="text" data-parent="Features" id="RewardRedemptionFeatures" class="form-control form-textarea" name="RewardRedemption" style=" min-height: 140px;" required> </textarea>
                                                        </div>
                                                    </div> 
                                                    <label for="CardApplication" class=" mt-4 text-primary ">Card Application</label>
                                                    <div class="selectBox row">
                                                        <div class="row">
                                                            <div class="col-3">
                                                                <label for="Applynowsequence" class="form-label">Apply Sequence</label>
                                                                <input type="number" class="form-control" id="Applynowsequence" name="Applynowsequence" required>
                                                            </div>
                                                            <div class="col-2">
                                                                <div class="d-flex flex-column align-items-center">
                                                                    <label for="applyNow" class="form-label ">Apply Now</label>
                                                                    <label class="switch">
                                                                        <input type="checkbox" id="applyNow">
                                                                        <span class="slider round"></span>
                                                                    </label>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row mt-4">
                                                            <div class="col-12">
                                                                <label for="Apply_Link" class="form-label" id="Apply_link_label">Apply Link</label>
                                                                <input type="text" class="form-control" id="Apply_Link" name="Apply_Link" required>
                                                            </div>
                                                            
                                                        </div>
                                                        <div class="row mt-4">
                                                            <div class="col-12">
                                                                <label for="featured_text" class="form-label" id="featured_text_label">Featured Text</label>
                                                                <input type="text" class="form-control" id="featured_text" name="featured_text" required>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-4 " style="padding-left: 28px!important;">
                                                    <div class="col-md-12 ">
                                                        <div class="requiredWrapper">
                                                            <div class=""><span class="required">*</span> <span>Required</span></div>
                                                            <!-- <div class=""><span class="oneofrequired">**</span><span>One of the marked is required</span></div> -->
                                                            
                                                        </div>
                                                        <div class=" form-group d-flex flex-column gap-3 mt-4 " id="multiSelectWrapper">
                                                            <div class="selectBox " id="cardIssuersSelectBox">
                                                                <label class="multiInputLabel" id="cardIssuersLabel">
                                                                    Card Issuer
                                                                </label>
                                                                <span class="required">*</span>
                                                                <input type="text" name="cardIssuersInput" id="cardIssuersInput" class="multiSelectInput form-control" placeholder="Select a Card Issuer" autocomplete="off" />
                                                                <button class="material-icons material-symbols-outlined input-expand-icon" id="cardIssuersDropdownIcon">
                                                                    expand_more
                                                                </button>
                                                                <div id="cardIssuersDropdown" class="selectBoxDropdown dropdownHidden "></div>
                                                                <div class="selectBoxSelected" id="cardIssuersSelectBoxSelected"></div>
                                                            </div>
                                                            <div class="selectBox " id="loungeNetworksSelectBox">
                                                                <label class="multiInputLabel" id="loungeNetworksLabel">
                                                                    Lounge Network
                                                                </label>
                                                                <input type="text" name="loungeNetworksInput" id="loungeNetworksInput" class="multiSelectInput form-control" placeholder="Select a Lounge Network" autocomplete="off" />
                                                                <button class="material-icons material-symbols-outlined input-expand-icon" id="loungeNetworksDropdownIcon">
                                                                    expand_more
                                                                </button>
                                                                <div id="loungeNetworksDropdown" class="selectBoxDropdown dropdownHidden "></div>
                                                                <div class="selectBoxSelected" id="loungeNetworksSelectBoxSelected"></div>
                                                            </div>
                                                            <div class="selectBox " id="loungesSelectBox">
                                                                <label class="multiInputLabel" id="loungesLabel">
                                                                    Lounges (0)
                                                                </label>
                                                                <input type="text" name="loungesInput" id="loungesInput" class="multiSelectInput form-control" placeholder="Select Options" autocomplete="off" />
                                                                <button class="material-icons material-symbols-outlined input-expand-icon" id="loungesDropdownIcon">
                                                                    expand_more
                                                                </button>
                                                                <div id="loungesDropdown" class="selectBoxDropdown dropdownHidden "></div>
                                                                <div class="selectBoxSelected" id="loungesSelectBoxSelected"></div>
                                                            </div>
                                                            <div class="selectBox" id="offersSelectBox">
                                                                <label class="multiInputLabel" id="offersLabel">
                                                                    Offers (0)
                                                                </label>
                                                                <input type="text" name="offersInput" id="offersInput" class="multiSelectInput form-control" placeholder="Select Options" autocomplete="off" />
                                                                <button class="material-icons material-symbols-outlined input-expand-icon" id="offersDropdownIcon">
                                                                    expand_more
                                                                </button>
                                                                <div id="offersDropdown" class="selectBoxDropdown dropdownHidden "></div>
                                                                <div class="selectBoxSelected" id="offersSelectBoxSelected"></div>
                                                            </div>
                                                        </div>
                                                        <div class="shadowBox cardNetworkBox">
                                                            <label class="multiInputLabel">
                                                                    Credit Card Type <span class="required">*</span>
                                                                </label>
                                                            <ul class="nav nav-pills mb-3" id="pills-tab-card-networks" role="tablist">
                                                                <li class="nav-item" role="presentation">
                                                                    <button class="nav-link active" id="pills-Visa-tab" data-bs-toggle="pill" data-bs-target="#pills-Visa" type="button" role="tab">Visa</button>
                                                                </li>
                                                                <li class="nav-item" role="presentation">
                                                                    <button class="nav-link" id="pills-Rupay-tab" data-bs-toggle="pill" data-bs-target="#pills-Rupay" type="button" role="tab">Rupay</button>
                                                                </li>
                                                                <li class="nav-item" role="presentation">
                                                                    <button class="nav-link" id="pills-MasterCard-tab" data-bs-toggle="pill" data-bs-target="#pills-MasterCard" type="button" role="tab">Master Card</button>
                                                                </li>
                                                                <li class="nav-item" role="presentation">
                                                                    <button class="nav-link" id="pills-AMEX-tab" data-bs-toggle="pill" data-bs-target="#pills-AMEX" type="button" role="tab">Amex</button>
                                                                </li>
                                                                <li class="nav-item" role="presentation">
                                                                    <button class="nav-link" id="pills-DinersClub-tab" data-bs-toggle="pill" data-bs-target="#pills-DinersClub" type="button" role="tab">DinersClub</button>
                                                                </li>
                                                            </ul>
                                                            <div class="tab-content col-md-12" id="pills-tabContent">
                                                                <div class="tab-pane fade show active" id="pills-Visa" role="tabpanel">
                                                                    <label for="" class="form-label">Visa</label>
                                                                    <select class="form-select form-control" id="Visa-select" name="Visa">
                                                                        <option value="" selected>Choose here</option>
                                                                        <option value="VisaClassic">VisaClassic</option>
                                                                        <option value="VisaGold">VisaGold</option>
                                                                        <option value="VisaPlatinum">VisaPlatinum</option>
                                                                        <option value="VisaSignature">VisaSignature</option>
                                                                        <option value="VisaInfinite">VisaInfinite</option>
                                                                    </select>
                                                                </div>
                                                                <div class="tab-pane fade " id="pills-Rupay" role="tabpanel" aria-labelledby="pills-Rupay-tab">
                                                                    <label for="" class="form-label">Rupay</label>
                                                                    <select class="form-select form-control" id="Rupay-select" name="Rupay">
                                                                        <option value="" selected>Choose here</option>
                                                                        <option value="Classic">Classic</option>
                                                                        <option value="Platinum">Platinum</option>
                                                                        <option value="Select">Select</option>
                                                                    </select>
                                                                </div>
                                                                <div class="tab-pane fade " id="pills-MasterCard" role="tabpanel" aria-labelledby="pills-MasterCard-tab">
                                                                    <label for="" class="form-label">Master Card</label>
                                                                    <select class="form-select form-control" id="MasterCard-select" name="masterCard">
                                                                        <option value="" selected>Choose here</option>
                                                                        <option value="Standard">Standard</option>
                                                                        <option value="World">World</option>
                                                                        <option value="WorldElite">WorldElite</option>
                                                                    </select>
                                                                </div>
                                                                <div class="tab-pane fade " id="pills-AMEX" role="tabpanel" aria-labelledby="pills-AMEX-tab">
                                                                    <div class="d-flex flex-column align-items-start ">
                                                                        <label for="AMEX" class="form-label ">American Express</label>
                                                                        <label class="switch">
                                                                            <input type="checkbox" id="AMEX-checkbox">
                                                                            <span class="slider round"></span>
                                                                        </label>
                                                                    </div>

                                                                </div>
                                                                <div class="tab-pane fade " id="pills-DinersClub" role="tabpanel" aria-labelledby="pills-DinersClub-tab">
                                                                    <div class="d-flex flex-column align-items-start ">
                                                                        <label for="dinersClub" class="form-label ">Diners Club</label>
                                                                        <label class="switch">
                                                                            <input type="checkbox" id="DinersClub-checkbox">
                                                                            <span class="slider round"></span>
                                                                        </label>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Javascripts -->
                <script src="./admin/assets/plugins/jquery/jquery-3.5.1.min.js"></script>
                <script src="./admin/assets/plugins/bootstrap/js/popper.min.js"></script>
                <script src="./admin/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
                <script src="./admin/assets/plugins/perfectscroll/perfect-scrollbar.min.js"></script>
                <script src="./admin/assets/plugins/pace/pace.min.js"></script>
                <script src="./admin/assets/plugins/highlight/highlight.pack.js"></script>
                <script src="./admin/assets/plugins/summernote/summernote-lite.min.js"></script>
                <script src="./admin/assets/js/main.min.js"></script>
                <script src="./admin/assets/js/custom.js"></script>
                <script src="./admin/assets/js/utils/blockModalsEsacping.js"></script>
<script src="./admin/assets/js/utils/multiUniSelectObject.js"></script>
                <script src="./admin/assets/js/commonNew.js"></script>
                <script src="./admin/assets/js/newCreditCard.js"></script>

</body>

</html>