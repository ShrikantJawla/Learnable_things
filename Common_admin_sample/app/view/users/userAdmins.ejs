<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../common/commonHeader'); %>
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css"
            integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g=="
            crossorigin="anonymous" referrerpolicy="no-referrer" />
        <link href="./admin/assets/plugins/flatpickr/flatpickr.min.css" rel="stylesheet" />
</head>


<body>


    <!-- add new admin user modal here...... -->

    <div class="modal fade" id="ciAddNewAdminUserForm" tabindex="-1" aria-labelledby="exampleModalLabel"
        aria-hidden="true">
        <div class="modal-dialog modal-xl">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">
                        Add New Admin User
                    </h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"
                        onclick="$('#errorShow').html('')"></button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-6">
                            <label for="adminName" class="form-label">User Name*</label>
                            <input type="text" class="form-control" id="adminUserName" name="adminUserName" required />
                        </div>
                        <div class="col-md-6">
                            <label for="adminEmail" class="form-label">Email*</label>
                            <input type="email" class="form-control" id="adminEmail" name="adminEmail" required />
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-md-4">
                            <label for="adminPassword" class="form-label">Password*</label>
                            <input type="password" class="form-control" id="adminPassword" name="adminPassword"
                                required />
                        </div>
                        <div class="col-md-4">
                            <label for="teleNumber" class="form-label">Tele Number</label>
                            <div class="number-wrapper">
                                <span class="country-code">+91</span>
                                <input type="text" class="form-control" id="teleNumber" name="teleNumber" required />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <label for="" class="form-label">User Role*</label>
                            <select class="form-select" aria-label="Select User Roles" id="userAdminRolesHere" required>
                                <option value="0" selected>Select User Role</option>
                            </select>
                        </div>

                    </div>
                    <div class="row">

                        <div class="col-md-2 py-3">
                            <div class="form-check">

                                <label class="form-check-label" for="new_active_user">
                                    Active user
                                </label>
                                <input class="form-check-input" type="checkbox" value="" class="form-check-input"
                                    name="new_active_user" id="new_active_user" required>
                            </div>

                        </div>
                    </div>
                    <br />
                    <div id="errorShow"></div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal"
                        onclick="$('#errorShow').html('')">
                        Cancel
                    </button>
                    <button type="button" id="ciFormModalBtnEdit" class="ci-btn" onclick="USERADMINS.addNewAdminUser()">
                        Add User
                    </button>
                </div>
            </div>
        </div>
    </div>

    <!-- update  admin user modal here...... -->

    <div class="modal fade" id="ciUpdateAdminUserForm" tabindex="-1" aria-labelledby="exampleModalLabel"
        aria-hidden="true">
        <div class="modal-dialog modal-xl">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">
                        Update Admin User
                    </h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"
                        onclick="$('#errorShowu').html('')"></button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-6">
                            <label for="adminName" class="form-label">User Name*</label>
                            <input type="text" class="form-control" id="adminUserNameu" name="adminUserNameu"
                                required />
                        </div>
                        <div class="col-md-6">
                            <label for="adminEmail" class="form-label">Email*</label>
                            <input type="email" class="form-control" id="adminEmailu" name="adminEmailu" required />
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-md-4">
                            <label for="adminPassword" class="form-label">Password*</label>
                            <input type="password" class="form-control" id="adminPasswordu" name="adminPasswordu"
                                required />
                        </div>
                        <div class="col-md-4">
                            <label for="teleNumberu" class="form-label">Tele Number</label>
                            <div class="number-wrapper">
                                <span class="country-code">+91</span>
                                <input type="text" class="form-control" id="teleNumberu" name="teleNumberu" required />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <label for="" class="form-label">User Role*</label>
                            <select class="form-select" aria-label="Select User Roles" name="userAdminRolesHereu"
                                id="userAdminRolesHereu" required>
                                <option value="0" selected>Select User Role</option>
                            </select>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-2 py-3">
                            <div class="form-check">

                                <label class="form-check-label" for="edit_active_user">
                                    Active user
                                </label>
                                <input class="form-check-input" type="checkbox" value="" class="form-check-input"
                                    name="active_user" id="edit_active_user" required>
                            </div>

                        </div>
                    </div>
                    <br />
                    <div id="errorShowu"></div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal"
                        onclick="$('#errorShowu').html('')">
                        Cancel
                    </button>
                    <button type="button" id="ciFormModalBtnEditu" class="ci-btn"
                        onclick=" USERADMINS.updateAdminUser()">
                        Update
                    </button>
                </div>
            </div>
        </div>
    </div>


    <div class="app align-content-stretch d-flex flex-wrap">
        <%- include('../common/commonLoader'); %>
            <%- include('../common/commonSidebar' , {sidebarData : sidebarDataByServer}); %>

                <div class="app-container ci-table-padding">
                    <div class="d-flex flex-column">
                        <div class="app-content">
                            <h1 class="application-title pb-1">
                                Admin Users
                            </h1>
                        </div>
                        <div style="
                            display: flex;
                            margin-bottom: 10px;
                            gap: 10px;
                            margin-left: auto;
                        ">

                            <button class="btn btn-primary" data-bs-toggle="modal"
                                data-bs-target="#ciAddNewAdminUserForm">
                                Add Item
                            </button>
                        </div>
                    </div>
                    <div class="card ci-scrollX">
                        <header class="card-header d-flex justify-content-between">
                            <div class="d-flex align-items-center" style="margin-bottom: 20px; gap: 12px">
                                <select name="entriesPerPage" id="entriesPerPage">
                                    <option value="10">10</option>
                                    <option value="25">25</option>
                                    <option value="50">50</option>
                                    <option value="100">100</option>
                                    <option value="200">200</option>
                                </select>
                                <label for="entriesPerPage" style="color: rgb(128, 128, 128)">
                                    entries / page</label>
                            </div>


                        </header>

                        <table class="table">
                            <thead style="
                                color: rgb(
                                    128,
                                    128,
                                    128
                                ) !important;
                            ">
                                <tr style="text-align: center" id="table-header" data-filter-value="0"
                                    data-filter="none">
                                    <!--id-->
                                    <th data-filter-value="0" data-filter="ua_id"
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="ua_id">User Id
                                            </span>
                                            <icon data-filter="ua_id"
                                                class="material-icons material-symbols-outlined invisible" style="
                                            cursor: pointer;
                                            
                                        ">
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    <!-- ua_name -->
                                    <th data-filter-value="0" data-filter="ua_name"
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="ua_name">User Name</span>
                                            <icon data-filter="ua_name"
                                                class="material-icons material-symbols-outlined invisible" style="
                                                cursor: pointer;
                                                
                                            ">
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    <!-- ua_email -->
                                    <th data-filter-value="0" data-filter="ua_email"
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="ua_email">User Email
                                            </span>
                                            <icon data-filter="ua_email"
                                                class="material-icons material-symbols-outlined invisible" style="
                                            cursor: pointer;
                                            
                                        ">
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    <!-- uar_role_name -->
                                    <th data-filter-value="0" data-filter="uar_role_name"
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="uar_role_name">User Role
                                            </span>
                                            <icon data-filter="uar_role_name"
                                                class="material-icons material-symbols-outlined invisible" style="
                                                cursor: pointer;
                                                
                                            ">
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    <!-- ua_tele_number -->
                                    <th data-filter-value="0" data-filter="ua_tele_number"
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="ua_tele_number">Tele Number
                                            </span>
                                            <icon data-filter="ua_tele_number"
                                                class="material-icons material-symbols-outlined invisible" style="
                                                cursor: pointer;
                                                
                                            ">
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    <!-- active user -->
                                    <th data-filter-value="0" data-filter="active_user"
                                        style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="active_user">User Active
                                            </span>
                                            <icon data-filter="active_user"
                                                class="material-icons material-symbols-outlined invisible" style="
                                                cursor: pointer;
                                                
                                            ">
                                                straight
                                            </icon>
                                        </div>
                                    </th>
                                    <!-- Actions -->
                                    <th data-filter-value="0" style="color: rgb(128, 128, 128) !important">
                                        <div class="d-flex ">
                                            <span style="cursor: pointer" data-filter="Actions">Actions</span>
                                        </div>
                                    </th>
                                </tr>
                            </thead>
                            <tbody id="data-to-show">
                                <tr>
                                    <td colspan="10">
                                        <div class="d-flex justify-content-center align-items-center">
                                            <!-- <div class="sbl-circ-path"></div> -->
                                        </div>
                                    </td>
                                </tr>
                            </tbody>
                            <tfoot>
                                <tr>
                                    <td colspan="10" style="
                                        text-align: center;
                                        color: black !important;
                                    ">
                                        <div
                                            class="display: flex; align-items: center; justify-content: center; flex-direction: column;">
                                            <%# prevPageBtn %>
                                                <span class="material-icons material-symbols-outlined" style="
                                                position: relative;
                                                top: 4px;
                                                right: 8px;
                                                font-size: 18px;
                                                color: white;
                                                background: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                                padding: 2px;
                                                border-radius: 50%;
                                                cursor: pointer;
                                            " id="prevPage">
                                                    west
                                                </span>
                                                <%# total pages label %>
                                                    <label for="pageNo" style="
                                                margin-right: 10px;
                                                color: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                            ">Page No:</label>
                                                    <%# input %>
                                                        <input type="number" style="
                                                width: 50px;
                                                display: inline;
                                                height: 20px;
                                                border-radius: 0 !important;
                                                padding-right: 0 !important;
                                                padding-left: 2px !important;
                                            " id="page-no" name="pageNo" class="form-control" max="50" min="1"
                                                            value="1" />
                                                        <%# /50 %>

                                                            <span style="
                                                margin-left: 10px;
                                                color: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                            " id="pages">/ 50</span>
                                                            <%# no of entries %>

                                                                <span style="
                                                margin-left: 10px;
                                                color: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                            " id="entries">(2000 entries)</span>

                                                                <span class="material-icons material-symbols-outlined"
                                                                    style="
                                                position: relative;
                                                top: 4px;
                                                left: 8px;
                                                font-size: 18px;
                                                color: white;
                                                background: rgb(
                                                    128,
                                                    128,
                                                    128
                                                ) !important;
                                                padding: 2px;
                                                border-radius: 50%;
                                                cursor: pointer;
                                            " id="nextPage">
                                                                    east
                                                                </span>
                                        </div>
                                    </td>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
                <!-- Javascripts -->

                <script src="/./app/utils/assets/plugins/jquery/jquery-3.5.1.min.js"></script>
                <script src="/./app/utils/assets/plugins/bootstrap/js/popper.min.js"></script>
                <script src="/./app/utils/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
                <script src="/./app/utils/assets/plugins/perfectscroll/perfect-scrollbar.min.js"></script>
                <script src="/./app/utils/assets/js/main.min.js"></script>
                <script src="/./app/utils/assets/js/custom.js"></script>
                <script src="/./app/utils/assets/js/custom/adminUsers.js"></script>
                <script src="/./app/utils/assets/js/custom/adminUsersNewEdit.js"></script>
</body>

</html>